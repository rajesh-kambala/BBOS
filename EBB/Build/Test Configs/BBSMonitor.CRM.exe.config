<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <appSettings>
        <add key="BBSMonitorEventName0"		value="PRCo.BBS.BBSMonitor.FileMoveEvent"/>

        <add key="FileMoveInterval"				value="5"/>
        <add key="FileMoveStartDateTime"		value="3/12/2020 12:05 am"/>
        <add key="FileMoveFolderCount"			value="4"/>
        <add key="FileMoveSourceFolderName0"	value="D:\Applications\CRM\WWWRoot\TempReports\Alerts"/>
        <add key="FileMoveTargetFolderName0"	value="\\Az-nc-bbos-q1\Campaigns\Alerts"/>
        <add key="FileMoveSourceFolderName1"	value="D:\Applications\CRM\WWWRoot\TempReports\Invoice"/>
        <add key="FileMoveTargetFolderName1"	value="\\Az-nc-bbos-q1\Campaigns\Invoice"/>
        <add key="FileMoveSourceFolderName2"	value="D:\Applications\CRM\WWWRoot\TempReports\TES"/>
        <add key="FileMoveTargetFolderName2"	value="\\Az-nc-bbos-q1\Campaigns\TES"/>
        <add key="FileMoveSourceFolderName3"	value="D:\Applications\CRM\WWWRoot\TempReports\Surveys"/>
        <add key="FileMoveTargetFolderName3"	value="\\Az-nc-bbos-q1\Campaigns\Surveys"/>


        <!-- Settings for the trace facility -->
        <add key="TraceServiceProvider"       value="TSI.Utils.FileLogger"/>
        <add key="TraceFileName"	          value="D:\Applications\BBSMonitor\BBSMonitor Trace.txt"/>
        <add key="TraceErrorFileName"         value="D:\Applications\BBSMonitor\BBSMonitor Error.txt"/>
        <add key="TraceLevel"			      value="1"/>
        <add key="TraceEnabled"			      value="true"/>
        <add key="TraceTriggerMode"		      value="1"/>               <!-- 0=None, 1=FileSize, 2=Interval -->
        <add key="TraceFileSizeThreshold"     value="2097152"/>     <!-- In Bytes -->
        <add key="TraceFileIntervalThreshold" value="1440"/>        <!-- In Minutes -->
        <add key="TraceMaxFilesToKeep"        value="4"/>
        <add key="TraceEventLogEnabled"	      value="true"/>

        <!-- Must have four replacements.  They are:
		0 = Timestamp
		1 =	User ID
		2 = Request Name
		3 = Message
	     	-->
        <add key="TraceMessageFormat" 		value="{0}|{2}|{3}"/>
        <add key="TraceTimestampFormat" 	value="yyyy-MM-dd HH:mm:ss:fff"/>

        <!-- Settings for exception support -->
        <add key="EMailSupportEnabled" value="false" />
        <add key="EMailSupportAddress" value="supportbbos@bluebookservices.com" />
        <add key="EMailSupportSubject" value="BBOSMonitor Exception (QA)" />

        <!-- Settings for Email -->
        <add key="EmailSubjectPrefix"	value="[BBSMonitor on DevSage] "/>
        <add key="EmailSupportFrom"		value="qa.bluebookservices@bluebookservices.com"/>
        <add key="EMailFromAddress"		value="qa.bluebookservices@bluebookservices.com"/>
        <add key="EmailSupportTo"		value="supportbbos@bluebookservices.com"/>
        <add key="EMailSMTPServer"		value="smtp3.bluebookservices.com"/>

        <add key="ServiceName"		value="BBSMonitor"/>
    </appSettings>

    <connectionStrings>
        <add name="TSIUtils" connectionString="server=qasql.bluebookservices.local;User ID=accpac;Password=accpac;Initial Catalog=CRM;Application Name=BBOSMonitor;" providerName="TSI.DataAccess.DBSQLServerProvider" />
    </connectionStrings>


    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>

</configuration>
