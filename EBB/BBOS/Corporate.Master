                                <div class="footerText"><asp:HyperLink ID="hlCommunityStandards" NavigateUrl="https://www.producebluebook.com/community-standards/"  Target="website" runat="server" EnableViewState="false">COMMUNITY STANDARDS</asp:HyperLink></div>
                                <div class="footerText"><a href="<% =PRCo.BBOS.UI.Web.Configuration.CorporateWebSite %>/contact-us/"><%= Resources.Global.Footer_ContactUs %></a></div>
