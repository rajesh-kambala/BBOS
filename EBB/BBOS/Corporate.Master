        <script type="text/javascript" src="Scripts/bootstrap3/bootstrap.min.js"></script>
        <script type="text/javascript" src="Scripts/bootstrap3/bootbox.min.js"></script>
        <%# UIUtils.GetJavaScriptTag(UIUtils.JS_BBOS_FILE) %>
