  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndustryType">
            <Value>=Parameters!IndustryType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT comp_CompanyID as BBID,
       comp_Name as Company,
	   dbo.ufn_FormatPerson(pers_FirstName, pers_LastName, pers_MiddleName, NULL, pers_Suffix) as Person,
	   RTRIM(prwu_EMail) as Email,
	   COUNT(DISTINCT CAST(prwsat_CreatedDate as Date)) as [Day Count]
  FROM PRWebAuditTrail
       INNER JOIN PRWebUser ON prwsat_WebUserID = prwu_WebUserID
	   INNER JOIN Person_Link ON prwu_PersonLinkID = peli_PersonLinkID
	   INNER JOIN Person ON peli_PersonID = pers_PersonID
	   INNER JOIN Company ON prwu_BBID = comp_CompanyID
 WHERE prwsat_PageName LIKE 'api/%'
   AND prwsat_CreatedDate BETWEEN @StartDate AND DATEADD(day, 1, @EndDate)
   AND comp_PRIndustryType IN (@IndustryType)
   AND comp_CompanyID NOT IN (SELECT CAST(capt_code as Int) FROM Custom_Captions WHERE capt_family = 'InternalHQID')
GROUP BY  comp_CompanyID,
       comp_Name,
	   dbo.ufn_FormatPerson(pers_FirstName, pers_LastName, pers_MiddleName, NULL, pers_Suffix),
	   prwu_EMail
ORDER BY Company, Person</CommandText>
      </Query>
