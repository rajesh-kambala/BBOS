<?xml version="1.0" encoding="utf-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <Description>Displays the summary of the image advertising campaign activity</Description>
  <Author>Travant Solutions, Inc.</Author>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="BBOS">
      <DataSourceReference>BBOS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>39d2df55-dc8c-4c44-9dfa-ab4c98511066</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="dsCountByCampaign">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ActiveEntirePeriod">
            <Value>=Parameters!ActiveEntirePeriod.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ComputedEndDate datetime
SET @ComputedEndDate = DATEADD(second, -1, DATEADD(day, 1, @ToDate))

SELECT pradcats_AdCampaignID, 
  pradc_Name, 
  comp_PRBookTradestyle,
  pradc_StartDate,
  pradc_EndDate,
  pradc_ImpressionCount,
  pradc_ClickCount,
  SUM(pradcats_ImpressionCount) AS PeriodImpressionCount,
  SUM(pradcats_ClickCount) AS PeriodClickCount
FROM PRAdCampaignAuditTrailSummary WITH (NOLOCK)
          INNER JOIN PRAdCampaign WITH (NOLOCK) ON pradcats_AdCampaignID = pradc_AdCampaignID
          INNER JOIN Company WITH (NOLOCK) ON pradc_CompanyID = comp_CompanyID
WHERE pradcats_DisplayDate BETWEEN @FromDate AND @ComputedEndDate
     AND pradc_AdCampaignType IN ('IA', 'SA', 'IA_180x90', 'IA_180x570', 'IA_960x100')
     AND pradc_StartDate &lt;= CASE @ActiveEntirePeriod WHEN 1 THEN @FromDate ELSE pradc_StartDate END
     AND pradc_EndDate &gt;= CASE @ActiveEntirePeriod WHEN 1 THEN @ComputedEndDate ELSE pradc_EndDate END
GROUP BY pradcats_AdCampaignID, pradc_Name, comp_PRBookTradestyle, pradc_StartDate, pradc_EndDate, pradc_ImpressionCount, pradc_ClickCount
ORDER BY comp_PRBookTradestyle</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
