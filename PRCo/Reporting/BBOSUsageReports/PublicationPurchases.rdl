          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="prreq_RequestTypeCode">
          <DataField>prreq_RequestTypeCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RequestTypeValue">
          <DataField>RequestTypeValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PurchaseCount">
          <DataField>PurchaseCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <CommandText>DECLARE @ComputedEndDate datetime
SET @ComputedEndDate = DATEADD(second, -1, DATEADD(day, 1, @ToDate))

SELECT prpbar_Name, 
  prpbed_Name, 
  prpbar_PublishDate, 
  prpbed_PublishDate, 
  prreq_RequestTypeCode,
  dbo.ufn_GetCustomCaptionValue('prreq_RequestTypeCode', prreq_RequestTypeCode, @Culture) AS RequestTypeValue,
  Count(1) As PurchaseCount
FROM PRRequest
    INNER JOIN PRRequestDetail ON prreq_RequestID = prrc_RequestID
    INNER JOIN PRPublicationArticle ON prrc_AssociatedID = prpbar_PublicationArticleID
    LEFT OUTER JOIN PRPublicationEdition ON prpbar_PublicationEditionID = prpbed_PublicationEditionID
WHERE prreq_RequestTypeCode in ('BP', 'BPO')
  AND prreq_CreatedDate BETWEEN @FromDate AND @ComputedEndDate
GROUP BY prreq_RequestTypeCode, prpbar_Name, prpbed_Name, prpbar_PublishDate, prpbed_PublishDate
ORDER BY prreq_RequestTypeCode, PurchaseCount DESC, prpbar_Name</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Culture">
            <Value>=Parameters!Culture.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
