    <DataSource Name="CRMArchive">
      <DataSourceReference>CRMArchive</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>dd6ecbf4-5f20-4212-852d-3758871b10b7</rd:DataSourceID>
