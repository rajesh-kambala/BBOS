  </DataSources>
  <DataSets>
    <DataSet Name="BBS">
      <Query>
        <DataSourceName>CRMArchive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PersonID">
            <Value>=Parameters!PersonID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Comm_CommunicationId AS CommID, 
       RTRIM(a.User_Logon) AS [Assigned To], 
       ISNULL(RTRIM(b.User_Logon), '') AS [Created by], 
       comm_DateTime as [Due Date],
	   Comm_Subject [Subject],
       LEFT(CAST(Communication.Comm_Note AS nvarchar(MAX)), 150) AS [Note Summary], 
       CASE 
            WHEN b.User_UserID &lt; 1 THEN 'System' 
            WHEN b.User_UserID IS NULL THEN 'System' 
            ELSE 'User' END AS TaskType,
      CASE WHEN  libr_FileName IS NULL THEN 'N' ELSE 'Y' END As HasAttachment,
      CRM.dbo.ufn_GetCapt_US('Choices', 'comm_PRCategory',comm_PRCategory) As [Category], 
      CRM.dbo.ufn_GetCapt_US('Choices', 'comm_PRSubCategory',comm_PRSubCategory) as [Sub Category]
  FROM CRMArchive.dbo.Communication 
       INNER JOIN CRMArchive.dbo.Comm_Link ON Comm_CommunicationId = CmLi_Comm_CommunicationId 
       LEFT OUTER JOIN CRM.dbo.Users a ON CmLi_Comm_UserId = a.User_UserId 
       LEFT OUTER JOIN CRM.dbo.Users b ON Comm_CreatedBy = b.User_UserId
       LEFT OUTER JOIN CRM.dbo.Library ON Comm_CommunicationId = libr_CommunicationID
 WHERE CmLi_Comm_CompanyId = ISNULL(@CompanyID, CmLi_Comm_CompanyId)
   AND ISNULL(CmLi_Comm_PersonID, 0) = ISNULL(@PersonID, ISNULL(CmLi_Comm_PersonID, 0))
   AND Comm_Deleted IS NULL 
   AND CmLi_Deleted IS NULL
ORDER BY comm_DateTime DESC, b.User_Logon DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CommID">
          <DataField>CommID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
