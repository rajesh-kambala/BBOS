    <DataSet Name="dsHeader">
      <Query>
        <DataSourceName>CRMArchive</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PersonID">
            <Value>=Parameters!PersonID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @Name varchar(200)

IF (@CompanyID IS NOT NULL) BEGIN
     SELECT @Name = prcse_FullName
         FROM CRM.dbo.PRCompanySearch WITH (NOLOCK)
    WHERE prcse_CompanyId = @CompanyID;

END ELSE BEGIN

     SET @Name = CRM.dbo.ufn_FormatPersonById(@PersonID) + ' (' + CAST(@PersonID as varchar(10)) + ')';

END

SELECT @Name As Header</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
