  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndustryType">
            <Value>=Parameters!IndustryType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ComputedEndDate datetime
SET @ComputedEndDate = DATEADD(day, 1, @EndDate)

DECLARE @MyTable table (
	CompanyID int,
	UserLogon varchar(40),
	OrderDateTime datetime,
	OldServiceCode varchar(40),
	NewServiceCode varchar(40),
	UpDownGrade varchar(40)
)

INSERT INTO @MyTable
SELECT prsoat_SoldToCompany, prsoat_UserLogon, prsoat_CreatedDate, prsoat_ItemCode, NULL, CASE prsoat_CancelReasonCode WHEN 'C24' THEN 'Upgrade' ELSE 'Downgrade' END
 FROM PRSalesOrderAuditTrail WITH (NOLOCK)
      INNER JOIN MAS_PRC.dbo.CI_Item WITH (NOLOCK) ON prsoat_ItemCode = ItemCode 
	  INNER JOIN Company WITH (NOLOCK) ON prsoat_SoldToCompany = Comp_CompanyId
WHERE prsoat_CreatedDate BETWEEN @StartDate AND @ComputedEndDate
  AND prsoat_CancelReasonCode IN ('C24','C30', 'D40', 'D41', 'D42')
  AND UseInSO = 'Y'
  AND Category2 = 'PRIMARY'
  AND comp_PRIndustryType IN (@IndustryType)

UPDATE @MyTable
  SET NewServiceCode = prsoat_ItemCode
 FROM PRSalesOrderAuditTrail
      INNER JOIN MAS_PRC.dbo.CI_Item WITH (NOLOCK) ON prsoat_ItemCode = ItemCode 
WHERE CompanyID =  prsoat_SoldToCompany
  AND prsoat_CreatedDate BETWEEN @StartDate AND @ComputedEndDate
  AND prsoat_ActionCode = 'I'
  AND UseInSO = 'Y'
  AND Category2 = 'PRIMARY'
  AND DateDiff(minute, OrderDateTime, prsoat_CreatedDate) BETWEEN 0 AND 5


SELECT NewServiceCode as [New Service],
	   UserLogon as [Sales Person],
	   UpDownGrade as [Up/Down Grade],
	   COUNT(1) as Cnt
  FROM @MyTable
GROUP BY NewServiceCode, UserLogon, UpDownGrade</CommandText>
      </Query>
      <Fields>
        <Field Name="New_Service">
          <DataField>New Service</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sales_Person">
          <DataField>Sales Person</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Up_Down_Grade">
          <DataField>Up/Down Grade</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Cnt">
          <DataField>Cnt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsIndustryType">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT capt_code, capt_US
 FROM Custom_Captions
WHERE capt_family = 'comp_PRIndustryType'
ORDER BY capt_order, capt_us</CommandText>
      </Query>
