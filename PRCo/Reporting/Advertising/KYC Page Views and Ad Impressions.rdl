        <CommandText>DECLARE @AdjustedEndDate datetime = DATEADD(day, 1, @EndDate) 
DECLARE @MyTable table (
    ArticleID int,
    PostName varchar(100),
    Impressions int,
    Clicks int,
		PageViews int
)

INSERT INTO @MyTable
    SELECT DISTINCT(prkycc_ArticleID), prkycc_PostName, 0, 0, 0
    FROM PRKYCCommodity
    ORDER BY prkycc_PostName

DECLARE @tblClick table (
    PostName2 varchar(100),
    ClickCount int
)

INSERT INTO @tblClick
	SELECT prkycc_PostName, SUM(pradcats_ClickCount) ClickCount 
		FROM PRAdCampaignAuditTrailSummary
			INNER JOIN PRAdCampaign WITH (NOLOCK) ON pradcats_AdCampaignID = pradc_AdCampaignID
            INNER JOIN PRKYCCommodity WITH (NOLOCK) ON pradc_KYCCommodityID  = prkycc_KYCCommodityID
	WHERE pradcats_DisplayDate BETWEEN @StartDate AND @AdjustedEndDate
	GROUP BY prkycc_PostName
	HAVING SUM(pradcats_ClickCount) > 0

DECLARE @tblImpressions table (
    PostName3 varchar(100),
    ImpressionCount int
)

INSERT INTO @tblImpressions
	SELECT prkycc_PostName, SUM(pradcats_ImpressionCount) ImpressionCount 
		FROM PRAdCampaignAuditTrailSummary
			INNER JOIN PRAdCampaign WITH (NOLOCK) ON pradcats_AdCampaignID = pradc_AdCampaignID
            INNER JOIN PRKYCCommodity WITH (NOLOCK) ON pradc_KYCCommodityID  = prkycc_KYCCommodityID
	WHERE pradcats_DisplayDate BETWEEN @StartDate AND @AdjustedEndDate
	GROUP BY prkycc_PostName
	HAVING SUM(pradcats_ImpressionCount) > 0

DECLARE @tblPageViews table (
    ArticleID2 int,
    PageViewCount int
)

INSERT INTO @tblPageViews
	SELECT prpar_PublicationArticleID, COUNT(*) PageViews 
		FROM PRPublicationArticleRead
		WHERE prpar_CreatedDate BETWEEN @StartDate AND @AdjustedEndDate
	GROUP BY prpar_PublicationArticleID
	
UPDATE @MyTable
   SET Clicks = ClickCount
  FROM @tblClick
 WHERE PostName = PostName2

UPDATE @MyTable
   SET Impressions = ImpressionCount
  FROM @tblImpressions
 WHERE PostName = PostName3

UPDATE @MyTable
   SET PageViews = PageViewCount
  FROM @tblPageViews
 WHERE ArticleID = ArticleID2

SELECT * 
  FROM @MyTable
ORDER BY PostName ASC</CommandText>
      </Query>
