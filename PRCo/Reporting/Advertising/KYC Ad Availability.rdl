    <DataSet Name="KYCEditions">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>select Capt_Code, Capt_US from Custom_Captions where Capt_Family='pradc_kycedition' order by capt_code desc</CommandText>
      </Query>
      <Fields>
        <Field Name="Capt_Code">
          <DataField>Capt_Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Capt_US">
          <DataField>Capt_US</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@KYCEdition">
            <Value>=Parameters!KYCEdition.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @MyTable table (
	ArticleID int,
	PostName varchar(100),
	Placement varchar(50),
	CompanyID_1 int,
	CompanyName_1 nvarchar(50),
	Location_1 varchar(1000),
	EndDate_1 datetime,
	CompanyID_2 int,
	CompanyName_2 nvarchar(50),
	Location_2 varchar(1000),
	EndDate_2 datetime,
	CompanyID_3 int,
	CompanyName_3 nvarchar(50),
	Location_3 varchar(1000),
	EndDate_3 datetime,
	Notes nvarchar(max)
)

INSERT INTO @MyTable
	SELECT DISTINCT(prkycc_ArticleID), prkycc_PostName, NULL, NULL, NULL, NULL, NULL, NULL, NULL, NULL, NULL, NULL, NULL, NULL, NULL, NULL
	FROM PRKYCCommodity
	ORDER BY prkycc_PostName

	UPDATE @MyTable 
		SET 
			CompanyID_1 = comp_CompanyID, 
			CompanyName_1 = comp_Name, 
			Location_1 = CityStateCountryShort, 
			EndDate_1 = pradc_EndDate,
			Notes = pradc_Notes
		FROM PRAdCampaign 
				INNER JOIN PRKYCCommodity ON pradc_KYCCommodityID = prkycc_KYCCommodityID
				INNER JOIN Company on Comp_CompanyId = pradc_CompanyID
				INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
		WHERE pradc_Placement LIKE 'KYCPage1'
			AND pradc_KYCEdition = @KYCEdition
			AND prkycc_ArticleID = ArticleID

	UPDATE @MyTable 
		SET 
			CompanyID_2 = comp_CompanyID, 
			CompanyName_2 = comp_Name, 
			Location_2 = CityStateCountryShort,
			EndDate_2 = pradc_EndDate
		FROM PRAdCampaign
			INNER JOIN PRKYCCommodity ON pradc_KYCCommodityID = prkycc_KYCCommodityID
			INNER JOIN Company on Comp_CompanyId = pradc_CompanyID
			INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
		WHERE pradc_Placement LIKE 'KYCPage2'
			AND pradc_KYCEdition = @KYCEdition
			AND prkycc_ArticleID = ArticleID

	UPDATE @MyTable 
		SET 
			CompanyID_3 = comp_CompanyID, 
			CompanyName_3 = comp_Name, 
			Location_3 = CityStateCountryShort,
			EndDate_3 = pradc_EndDate
		FROM PRAdCampaign
			INNER JOIN PRKYCCommodity ON pradc_KYCCommodityID = prkycc_KYCCommodityID
			INNER JOIN Company on Comp_CompanyId = pradc_CompanyID
			INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
		WHERE pradc_Placement LIKE 'KYCPage3'
			AND pradc_KYCEdition = @KYCEdition
			AND prkycc_ArticleID = ArticleID

INSERT INTO @MyTable
SELECT 999, 'ZZ - Prem - ' + Capt_US , Capt_Code, NULL, NULL, NULL, NULL, 999, NULL, NULL, NULL, NULL, NULL, NULL, NULL, NULL
	FROM Custom_Captions
	WHERE Capt_Family='pradc_PlacementKYC' AND Capt_Code NOT IN ('KYCPage1','KYCPage2','KYCPage3')
	ORDER BY Capt_US

UPDATE @MyTable
	SET		CompanyID_1 = Comp_CompanyId, 
				CompanyName_1 = Comp_Name,
				Location_1 = CityStateCountryShort,
				EndDate_1 = pradc_EndDate
	FROM PRAdCampaign
		INNER JOIN Company on Comp_CompanyId = pradc_CompanyID
		INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
		--INNER JOIN Custom_captions ON capt_code = pradc_Placement AND Capt_Family='pradc_PlacementKYC' AND Capt_Code NOT IN ('KYCPage1','KYCPage2','KYCPage3')
	WHERE 
		pradc_Placement = Placement
		AND pradc_KYCEdition = @KYCEdition

SELECT 
	ArticleID, 
	REPLACE(PostName, '&amp;amp;', '&amp;') PostName,
	Placement,
	CompanyID_1,
	CompanyName_1,
	Location_1,
	EndDate_1,
	CompanyID_2,
	CompanyName_2,
	Location_2,
	EndDate_2,
	CompanyID_3,
	CompanyName_3,
	Location_3,
	EndDate_3,
	Notes
FROM @MyTable
	ORDER BY PostName ASC</CommandText>
