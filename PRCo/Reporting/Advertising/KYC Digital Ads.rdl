  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@KYCEdition">
            <Value>=Parameters!KYCEdition.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT comp_CompanyID as [BBID],
     comp_Name as [Company Name],
	   RTRIM(user_logon) as [Sold By],
		 prkycc_PostName [Publication Article], 
		 ccAdCreativeStatus.capt_us AS [Creative Status],
	   ccAdPlacement.capt_us as [Ad Position],
	   pradc_StartDate,
	   pradc_EndDate,
		dbo.ufn_GetCustomCaptionValue('pradc_AdSize', pradc_AdSize, 'en-us') As pradc_AdSize,
		ISNULL(pradc_placementinstructions, '') as pradc_placementinstructions
FROM PRAdCampaignHeader
		INNER JOIN PRAdCampaign ON pradc_AdCampaignHeaderID = pradch_AdCampaignHeaderID
		INNER JOIN Company WITH (NOLOCK) ON pradc_CompanyID = comp_CompanyID
		LEFT OUTER JOIN PRKYCCommodity ON prkycc_KYCCommodityID = pradc_KYCCommodityID
		LEFT OUTER JOIN Custom_Captions ccAdPlacement ON pradc_Placement = ccAdPlacement.capt_code AND ccAdPlacement.capt_Family = 'pradc_Placement'
		LEFT OUTER JOIN Custom_Captions ccAdCreativeStatus ON pradc_CreativeStatus = ccAdCreativeStatus.capt_code AND ccAdCreativeStatus.capt_Family = 'pradc_CreativeStatus'
		LEFT OUTER JOIN Users WITH (NOLOCK) ON pradch_SoldBy = user_userid
WHERE 
	pradch_TypeCode = 'KYC'
	AND pradc_KYCEdition = @KYCEdition
    AND (pradc_StartDate IS NOT NULL AND pradc_EndDate IS NOT NULL) --KYCD equivalent
ORDER BY comp_Name</CommandText>
      </Query>
