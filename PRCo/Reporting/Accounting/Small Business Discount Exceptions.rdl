    <DataSet Name="dsUneligibleCompaniesWithDiscount">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SalesThreshold">
            <Value>=Parameters!SalesThreshold.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BillingMonth">
            <Value>=Parameters!BillingMonth.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @StatementDateThreshold date
SET @StatementDateThreshold = CONVERT(DATE, CAST(@BillingMonth as varchar(2)) + '/1/' + CAST(YEAR(GETDATE()) as varchar(4)), 101)
IF (@StatementDateThreshold &lt; CONVERT(DATE, CAST(MONTH(GETDATE()) as varchar(2)) + '/1/' + CAST(YEAR(GETDATE()) as varchar(4)), 101)) BEGIN
	SET @StatementDateThreshold = DATEADD(year, 1, @StatementDateThreshold)
END
SET @StatementDateThreshold = DATEADD(MONTH, -18, @StatementDateThreshold)

	SELECT c.Comp_CompanyID, 
	c.Comp_Name,
	c.comp_PRIndustryType
                 ,prs.SalesOrderNo
	,prs.BillingMonth
	,prs.prse_DiscountPct
	,prs.prse_Primary
	,prs.prse_ServiceCode
	,prs.ItemCodeDesc
	,	vprl.prcn_Country 
	,prf.prfs_StatementDate LastFinancialStatementDate
	,prf.prfs_Sales LastSalesAmt
	FROM dbo.Company c 
		INNER JOIN vPRLocation vprl ON vprl.prci_CityID = c.comp_PRListingCityId AND (vprl.prcn_CountryId IN (1,2))
		--INNER JOIN (SELECT RANK() OVER (PARTITION BY prfs_CompanyId ORDER BY prfs_StatementDate desc) r, * FROM PRFinancial) prf ON c.Comp_CompanyId = prf.prfs_CompanyId
		LEFT OUTER JOIN PRFinancial prf WITH (NOLOCK) ON  c.Comp_CompanyId = prf.prfs_CompanyId AND prf.prfs_StatementDate = c.comp_PRFinancialStatementDate
		INNER JOIN PRService prs ON prs.prse_HQID = c.Comp_CompanyID AND prs.prse_Primary = 'Y' AND ISNULL(prs.prse_DiscountPct,0) = 5.0
	WHERE
		c.comp_PRListingStatus = 'L'
		AND c.comp_PRIndustryType IN ('P', 'T')
		AND
		(
			prf.prfs_FinancialId IS NULL
			OR prf.prfs_StatementDate  &lt; @StatementDateThreshold
			OR ISNULL(prf.prfs_Sales, 0) &gt; @SalesThreshold
		)
		AND prcn_CountryID IN (1, 2)
		AND prs.BillingMonth = @BillingMonth
		--AND prf.r = 1
	ORDER BY Comp_CompanyId ASC</CommandText>
      </Query>
