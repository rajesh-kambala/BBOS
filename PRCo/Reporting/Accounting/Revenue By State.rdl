    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>			SELECT ShipToState, 
(SUM(TaxableSalesAmt) + SUM(NonTaxableSalesAmt) + SUM(SalesTaxAmt) - SUM(DiscountAmt)) Sales
				FROM MAS_PRC.dbo.AR_InvoiceHistoryHeader 
			 WHERE InvoiceDate BETWEEN @StartDate AND @EndDate 
				 AND ShipToCountryCode IN ('001','002')
				 --AND TaxSchedule &lt;&gt; 'NT'
			GROUP BY ShipToState</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ShipToState">
