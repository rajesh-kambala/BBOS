  <DataSources>
    <DataSource Name="BBS">
      <DataSourceReference>BBS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>bb1a58e9-78c6-44e9-81d5-d45456cb499e</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BillingCycle">
            <Value>=Parameters!BillingCycle.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
  SELECT c.comp_CompanyID, c.comp_Name, CityStateCountryShort, BillingMonth, 
        CASE c.comp_PRType WHEN 'B' THEN hq.comp_CompanyID ELSE NULL END as BillingID,
		CASE c.comp_PRType WHEN 'B' THEN hq.comp_Name ELSE NULL END as BillingName 
  FROM Company c WITH (NOLOCK)
       INNER JOIN vPRLocation ON c.comp_PRListingCityID = prci_CityID
       INNER JOIN PRCompanyIndicators WITH (NOLOCK) ON c.comp_CompanyID = prci2_CompanyID
	   INNER JOIN PRService ON c.comp_CompanyID = prse_CompanyID
	   INNER JOIN Company hq WITH (NOLOCK) ON hq.comp_CompanyID = c.comp_PRHQID
 WHERE prci2_BillingException = 'Y'
   AND prse_Primary = 'Y'
   AND BillingMonth IN (@BillingCycle)
 ORDER BY c.comp_CompanyID

</CommandText>
      </Query>
      <Fields>
        <Field Name="comp_CompanyID">
          <DataField>comp_CompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="comp_Name">
          <DataField>comp_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CityStateCountryShort">
          <DataField>CityStateCountryShort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BillingMonth">
          <DataField>BillingMonth</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BillingID">
          <DataField>BillingID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BillingName">
          <DataField>BillingName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
