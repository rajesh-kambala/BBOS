    <DataSet Name="dsHeader">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InvoiceNo">
            <Value>=Parameters!InvoiceNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          SELECT MAS.JournalNoGLBatchNo, MAS.UDF_MASTER_INVOICE, MAS.BillToCustomerNo, MAS.InvoiceDate, MAS.InvoiceDueDate, MAS.TermsCodeDesc, MAS.ContactName, MAS.BillToName, MAS.BillToAddress1, MAS.BillToAddress2, MAS.BillToAddress3, MAS.BillToCity, MAS.BillToState, MAS.BillToZipCode,	MAS.BillToCountryCode, MAS.CountryName, MAS.FaxNo, MAS.EmailAddress, MAS.DiscountAmt, MAS.TaxableSalesAmt, MAS.NonTaxableSalesAmt, MAS.SalesTaxAmt, MAS.Total, MAS.Balance, MAS.prci2_BillingException, MAS.prattn_IncludeWireTransferInstructions, MAS.HasBillAddress,
          dbo.ufn_HasAdvertisingItemCode(@InvoiceNo) HasAdvertisingItemCode,
          'AttnName' = CASE WHEN (prattn_PersonID IS NULL AND prattn_CustomLine IS NULL) THEN ContactName ELSE 'Attn: ' + ISNULL(prattn_CustomLine, dbo.ufn_FormatPerson(pers_FirstName, pers_LastName, pers_MiddleName, null, pers_Suffix)) END
          FROM    MAS_PRC.dbo.vBBSiMasterInvoices MAS
          LEFT OUTER JOIN Company WITH(NOLOCK) ON SUBSTRING(MAS.BillToCustomerNo,2,6) = Comp_CompanyId
          LEFT OUTER JOIN PRAttentionLine WITH (NOLOCK) ON prattn_CompanyID = SUBSTRING(MAS.BillToCustomerNo,2,6) AND prattn_ItemCode = 'ADVBILL'
          LEFT OUTER JOIN vPRAddress bl WITH (NOLOCK) ON prattn_AddressID = bl.Addr_AddressId
          LEFT OUTER JOIN vPerson pers WITH (NOLOCK) ON pers.Pers_PersonId = prattn_PersonID
          WHERE  (MAS.UDF_MASTER_INVOICE = @InvoiceNo)
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
