      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BillingMonth">
            <Value>=Parameters!BillingMonth.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	'BBID' = Comp_CompanyId
	, 'Company Name' = comp_PRCorrTradestyle
	, 'Location' = CityStateCountryShort
	, 'Exception' = prci2_BillingException
	, 'Service Count' = COUNT(1)
	, 'Location Count' = COUNT(DISTINCT prse_CompanyID)
	, 'Total Billed' = SUM(ExtensionAmt)
FROM crm.dbo.Company bill WITH (NOLOCK)
INNER JOIN crm.dbo.vPRLocation bloc WITH (NOLOCK) ON prci_CityID = comp_PRListingCityId
INNER JOIN crm.dbo.PRService svc WITH (NOLOCK) ON prse_BillToCompanyID = Comp_CompanyId
INNER JOIN crm.dbo.PRCompanyIndicators with (nolock) on prci2_CompanyID = prse_BillToCompanyID
WHERE convert(int,BillingMonth) = @BillingMonth
and svc.SalesKitItem != 'Y'
group by Comp_CompanyId, comp_PRCorrTradestyle, CityStateCountryShort, prci2_BillingException
Order by [Total Billed] desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
