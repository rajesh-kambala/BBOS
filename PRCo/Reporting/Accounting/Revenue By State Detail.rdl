  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ItemCodes">
            <Value>=Parameters!ItemCodes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProductLines">
            <Value>=Parameters!ProductLines.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TaxClasses">
            <Value>=Parameters!TaxClasses.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@States">
            <Value>=Parameters!States.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ShipToState, 
       d.ItemCode,
	   d.ItemCodeDesc,
	   d.ProductLine,
	   ProductLineDesc,
	   d.TaxClass,
	   TaxClassDesc,
	   FORMAT(InvoiceDate, 'MMM yyyy') MonthDisplay,
	   FORMAT(InvoiceDate, 'yyyyMM') MonthSort,
       SUM(d.ExtensionAmt) Revenue
  FROM MAS_PRC.dbo.AR_InvoiceHistoryHeader h
       INNER JOIN MAS_PRC.dbo.AR_InvoiceHistoryDetail d ON h.InvoiceNo = d.InvoiceNo
	   INNER JOIN MAS_PRC.dbo.CI_Item i ON d.ItemCode = i.ItemCode
	   INNER JOIN MAS_PRC.dbo.IM_ProductLine pl ON d.ProductLine = pl.ProductLine
	   INNER JOIN MAS_System.dbo.SY_SalesTaxClass tc ON d.TaxClass = tc.TaxClass
 WHERE InvoiceDate BETWEEN @StartDate AND @EndDate 
   AND ShipToCountryCode IN ('001','002')
   AND d.ExplodedKitItem &lt;&gt; 'C'
   AND d.ItemCode IN (@ItemCodes)
   AND d.ProductLine IN (@ProductLines)
   AND d.TaxClass IN (@TaxClasses)
   AND ShipToState in (@States)
GROUP BY ShipToState, 
       d.ItemCode,
	   d.ItemCodeDesc,
	   d.ProductLine,
	   ProductLineDesc,
	   d.TaxClass,
	   TaxClassDesc,
	   FORMAT(InvoiceDate, 'MMM yyyy'),
	   FORMAT(InvoiceDate, 'yyyyMM')
HAVING SUM(d.ExtensionAmt) &gt; 0 
ORDER BY ShipToState, ItemCode, FORMAT(InvoiceDate, 'yyyyMM')</CommandText>
      </Query>
