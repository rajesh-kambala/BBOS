  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@State">
            <Value>=Parameters!State.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT comp_CompanyID [BB #],
       comp_Name as [Account Name],
       pritr_City [City],
	   pritr_County [County],
	   pritr_State [State],
	   pritr_TaxRate [Sales Tax Rate],
       Total [Total Sales],
	   TaxableSalesAmt [Taxable],
	   NonTaxableSalesAmt [Non-taxable], 
	   SUM(pritr_TaxAmt) as [Sales Tax]
  FROM CRM.dbo.PRInvoiceTaxRate WITH (NOLOCK)
       INNER JOIN MAS_PRC.dbo.vBBSiMasterInvoices WITH (NOLOCK) ON pritr_MasterInvoiceNo = UDF_MASTER_INVOICE
	   INNER JOIN Company WITH (NOLOCK) ON BillToCustomerNo = comp_CompanyID
 WHERE pritr_CreatedDate BETWEEN @StartDate AND  DATEADD(day, 1, @EndDate)
   AND pritr_State = @State
   AND pritr_TaxAmt &gt; 0
 GROUP BY  comp_CompanyID,
       comp_Name,
       pritr_City,
	   pritr_County,
	   pritr_State,
	   pritr_TaxRate,
       Total,
	   TaxableSalesAmt,
	   NonTaxableSalesAmt 
ORDER BY comp_Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
