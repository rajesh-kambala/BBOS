    <DataSet Name="dsDetails">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InvoiceNo">
            <Value>=Parameters!InvoiceNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        det.JournalNoGLBatchNo, det.UDF_MASTER_INVOICE, det.BillToCustomerNo, det.ShipToCity, det.ShipToState, det.DetailSeqNo, det.ItemCode, det.ItemCodeDesc, 
                         det.ExplodedKitItem, det.QuantityOrdered, det.ExtensionAmt, det.UnitPrice, det.LineDiscountPercent, det.PrimarySortOrder, det.ListingCity, det.ListingState, 
                         det.prcn_Country, det.CustomerNo, PRCompanySearch.prcse_FullName AS ServiceLocation
FROM            MAS_PRC.dbo.vBBSiMasterInvoiceDetails AS det INNER JOIN
                         PRCompanySearch WITH (NOLOCK) ON PRCompanySearch.prcse_CompanyId = det.CustomerNo
WHERE        (det.UDF_MASTER_INVOICE = @InvoiceNo) AND (det.ExplodedKitItem &lt;&gt; 'C')
ORDER BY det.ShipToState, det.ShipToCity, det.DetailSeqNo</CommandText>
      </Query>
