    <DataSet Name="dsInvoice">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InvoiceNo">
            <Value>=Parameters!InvoiceNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP 1 prinv_InvoiceID, prinv_CompanyID, prinv_InvoiceNbr, prinv_StripePaymentURL, prinv_PaymentURLKey,
       (SELECT Capt_US FROM Custom_Captions with(nolock) WHERE Capt_Family='BBOS' AND capt_Code='URL') BBOS
  FROM PRInvoice with (nolock) 
 WHERE prinv_InvoiceNbr=@InvoiceNo
ORDER BY ISNULL(prinv_PaymentImportedIntoMAS, 'Z'), prinv_InvoiceID DESC</CommandText>
      </Query>
