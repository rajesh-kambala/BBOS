    <DataSet Name="dsAddress">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InvoiceNo">
            <Value>=Parameters!InvoiceNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Addr_Address1, Addr_Address2, Addr_Address3, prci_City, ISNULL(prst_Abbreviation, prst_State) As State, prcn_Country, Addr_PostCode
  FROM CRM.dbo.vPRAddress
       INNER JOIN MAS_PRC.dbo.vBBSIMasterInvoices ON BillToCustomerNo = adli_CompanyId
 WHERE adli_PRDefaultMailing='Y'
   AND UDF_MASTER_INVOICE = @InvoiceNo
