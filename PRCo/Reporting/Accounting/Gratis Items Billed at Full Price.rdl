    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>DECLARE @Gratis TABLE (StandardOrder INT, RepeatOrder INT, OrderTotal FLOAT, ActionStep VARCHAR(100), Item VARCHAR(100), BBID INT)

INSERT INTO @Gratis
SELECT
	std.SalesOrderNo
	, hdr.SalesOrderNo
	, OrderTotal = hdr.NonTaxableAmt + hdr.TaxableAmt + hdr.SalesTaxAmt
	, 'Delete Order'
	, 'All Services'
	, hdr.CustomerNo
FROM MAS_PRC.dbo.SO_SalesOrderHeader hdr WITH (NOLOCK)
LEFT OUTER JOIN MAS_PRC.dbo.SO_SalesOrderHeader std WITH (NOLOCK) ON std.MasterRepeatingOrderNo = hdr.SalesOrderNo
	AND std.OrderType = 'S'
WHERE
(hdr.NonTaxableAmt + hdr.TaxableAmt + hdr.SalesTaxAmt) = 0
AND std.SalesOrderNo IS NOT NULL
Order BY hdr.SalesOrderNo


INSERT INTO @Gratis (StandardOrder, Item, RepeatOrder, OrderTotal, ActionStep, BBID)
SELECT
	std.SalesOrderNo
	, det.ItemCode
	, hdr.SalesOrderNo
	, OrderTotal = hdr.NonTaxableAmt + hdr.TaxableAmt + hdr.SalesTaxAmt
	, 'Discount Item'
	, hdr.CustomerNo
FROM MAS_PRC.dbo.SO_SalesOrderHeader hdr WITH (NOLOCK)
INNER JOIN MAS_PRC.dbo.SO_SalesOrderDetail det ON hdr.SalesOrderNo = det.SalesOrderNo
LEFT OUTER JOIN MAS_PRC.dbo.SO_SalesOrderHeader std ON std.MasterRepeatingOrderNo = hdr.SalesOrderNo
	AND std.OrderType = 'S'
WHERE
	det.LineDiscountPercent = 100
	AND (hdr.NonTaxableAmt + hdr.TaxableAmt + hdr.SalesTaxAmt) != (std.NonTaxableAmt + std.TaxableAmt + std.SalesTaxAmt)
	AND hdr.NonTaxableAmt + hdr.TaxableAmt + hdr.SalesTaxAmt &gt; 0
Order BY hdr.SalesOrderNo

SELECT g.*, comp_name
FROM @Gratis g
INNER JOIN crm.dbo.Company WITH (NOLOCK) ON comp_CompanyID = BBID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
