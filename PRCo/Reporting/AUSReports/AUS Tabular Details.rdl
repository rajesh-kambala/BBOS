  <DataSets>
    <DataSet Name="dsCreditSheetItems">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ChangePreference">
            <Value>=Parameters!ChangePreference.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prcs_CreditSheetID, dbo.ufn_GetItem(prcs_CreditSheetID, 2, 0, 60) as ItemText
  FROM PRCreditSheet WITH (NOLOCK)
 WHERE prcs_CompanyID = @CompanyID
     AND prcs_PublishableDate BETWEEN @StartDate AND @EndDate
     AND prcs_Status = 'P' -- Publishable
     AND prcs_NewListing IS NULL
     AND ISNULL(prcs_KeyFlag, 'N') = CASE WHEN @ChangePreference ='1' THEN 'Y' ELSE ISNULL(prcs_KeyFlag, 'N')  END;</CommandText>
      </Query>
      <Fields>
        <Field Name="prcs_CreditSheetID">
          <DataField>prcs_CreditSheetID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ItemText">
          <DataField>ItemText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsNewsArticles">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prpbar_PublicationArticleID, 
             prpbar_Name, 
             dbo.ufn_ConcatURL(dbo.ufn_GetCustomCaptionValue('BBOS', 'URL', 'en-us'), 'NewsArticle.aspx?SourceID=0&amp;SourceEntityType=AUSReport&amp;ArticleID=') As URL
  FROM PRPublicationArticleCompany WITH (NOLOCK)
             INNER JOIN PRPublicationArticle WITH (NOLOCK) ON prpbarc_PublicationArticleID = prpbar_PublicationArticleID
WHERE prpbarc_CompanyID = @CompanyID
     AND prpbar_PublishDate BETWEEN @StartDate AND @EndDate
     AND prpbar_News='Y';</CommandText>
      </Query>
      <Fields>
        <Field Name="prpbar_PublicationArticleID">
          <DataField>prpbar_PublicationArticleID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prpbar_Name">
          <DataField>prpbar_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="URL">
          <DataField>URL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsBBSIClaims">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prss_RespondentCompanyId,
       prss_OpenedDate,
	   prss_SSFileID,
       dbo.ufn_ConcatURL(dbo.ufn_GetCustomCaptionValue('BBOS', 'URL', 'en-us'), 'CompanyDetailsClaimsActivity.aspx?CompanyID=') As URL
  FROM PRSSFile WITH (NOLOCK)
 WHERE prss_RespondentCompanyId = @CompanyID
   AND prss_OpenedDate BETWEEN @StartDate AND @EndDate</CommandText>
      </Query>
      <Fields>
        <Field Name="prss_RespondentCompanyId">
          <DataField>prss_RespondentCompanyId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prss_OpenedDate">
          <DataField>prss_OpenedDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="prss_SSFileID">
          <DataField>prss_SSFileID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="URL">
          <DataField>URL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCourtCases">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prcc_CompanyID,
       prcc_FiledDate,
	   prcc_CaseNumber,
       dbo.ufn_ConcatURL(dbo.ufn_GetCustomCaptionValue('BBOS', 'URL', 'en-us'), 'CompanyDetailsClaimsActivity.aspx?CompanyID=') As URL
  FROM PRCourtCases WITH (NOLOCK)
 WHERE prcc_CompanyID = @CompanyID
   AND prcc_CreatedDate BETWEEN @StartDate AND @EndDate
   AND prcc_FiledDate &gt; DATEADD(day, -45, GETDATE())</CommandText>
      </Query>
      <Fields>
        <Field Name="prcc_CompanyID">
          <DataField>prcc_CompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prcc_FiledDate">
