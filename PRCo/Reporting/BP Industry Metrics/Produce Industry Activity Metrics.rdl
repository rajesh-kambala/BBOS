  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT YEAR(prbe_EffectiveDate) as [Year],
       'Bankruptcy' as Metric, 
       COUNT(DISTINCT prbe_CompanyId) as Value
FROM PRBusinessEvent 
INNER JOIN Company ON comp_CompanyID = prbe_CompanyId
	AND comp_PRIndustryType != 'L'
WHERE (prbe_BusinessEventTypeId IN (4, 5, 6) 
OR prbe_PublishedAnalysis LIKE '%Bankrupt%')
AND prbe_EffectiveDate BETWEEN @StartDate AND @EndDate
GROUP BY YEAR(prbe_EffectiveDate)

UNION

SELECT YEAR(prbe_EffectiveDate) as [Year],
       'Merger', COUNT(DISTINCT prbe_CompanyId)
FROM PRBusinessEvent 
INNER JOIN Company ON comp_CompanyID = prbe_CompanyId
	AND comp_PRIndustryType != 'L'
WHERE prbe_BusinessEventTypeId = 22
AND prbe_EffectiveDate BETWEEN @StartDate AND @EndDate
GROUP BY YEAR(prbe_EffectiveDate)

UNION

SELECT YEAR(prbe_EffectiveDate) as [Year],
         'Acquisition' , COUNT(DISTINCT prbe_CompanyId)
FROM PRBusinessEvent 
INNER JOIN Company ON comp_CompanyID = prbe_CompanyId
	AND comp_PRIndustryType != 'L'
WHERE prbe_BusinessEventTypeId = 1
AND prbe_EffectiveDate BETWEEN @StartDate AND @EndDate
GROUP BY YEAR(prbe_EffectiveDate)

UNION

SELECT YEAR(prbe_EffectiveDate) as [Year], 'Established' , COUNT(DISTINCT prbe_CompanyId)
FROM PRBusinessEvent 
INNER JOIN Company ON comp_CompanyID = prbe_CompanyId
	AND comp_PRIndustryType != 'L'
WHERE prbe_BusinessEventTypeId = 9
AND prbe_EffectiveDate BETWEEN @StartDate AND @EndDate
GROUP BY YEAR(prbe_EffectiveDate)

UNION

SELECT T1.[Year], 'Closure', SUM(Num113 + Num88 + Num114) 
 FROM 
		(SELECT YEAR(prra_Date) as [Year], Num113 = COUNT(DISTINCT prra_CompanyId)
		FROM vPRCompanyRating
		INNER JOIN Company ON comp_CompanyID = prra_CompanyId
			AND comp_PRIndustryType != 'L'
		WHERE prra_Date BETWEEN @StartDate AND @EndDate
		AND prra_RatingLine LIKE '%(113)%'
		AND prra_CompanyId NOT IN (SELECT prra_CompanyId FROM vPRCompanyRating
				WHERE prra_RatingLine LIKE '%(113)%' AND prra_Date &lt; @StartDate)
		GROUP BY YEAR(prra_Date)) T1

		INNER JOIN  (
SELECT YEAR(prra_Date) as [Year], Num88 =COUNT(DISTINCT prra_CompanyId)
FROM vPRCompanyRating
INNER JOIN Company ON comp_CompanyID = prra_CompanyId
	AND comp_PRIndustryType != 'L'
WHERE prra_Date BETWEEN @StartDate AND @EndDate
AND prra_RatingLine LIKE '%(88)%'
AND prra_CompanyId NOT IN (SELECT prra_CompanyId FROM vPRCompanyRating
		WHERE prra_RatingLine LIKE '%(88)%' AND prra_Date &lt; @StartDate)
GROUP BY YEAR(prra_Date)) T2 ON T1.[Year] = T2.[Year]

INNER JOIN  (
SELECT YEAR(prra_Date) as [Year], Num114 =COUNT(DISTINCT prra_CompanyId)
FROM vPRCompanyRating
INNER JOIN Company ON comp_CompanyID = prra_CompanyId
	AND comp_PRIndustryType != 'L'
WHERE prra_Date BETWEEN @StartDate AND @EndDate
AND prra_RatingLine LIKE '%(114)%'
AND prra_CompanyId NOT IN (SELECT prra_CompanyId FROM vPRCompanyRating
		WHERE prra_RatingLine LIKE '%(114)%' AND prra_Date &lt; @StartDate)
GROUP BY YEAR(prra_Date)) T3 ON T1.[Year] = T3.[Year]

GROUP BY T1.[Year]</CommandText>
