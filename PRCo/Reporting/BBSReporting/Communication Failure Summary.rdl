    <DataSet Name="dsSource">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>DECLARE @tblSource table (
	Display varchar(100),
	Value varchar(100)
)

INSERT INTO @tblSource VALUES ('All (includes other not listed below)', '%');
INSERT INTO @tblSource VALUES ('AUS', '%AUS Report%');
INSERT INTO @tblSource VALUES ('BBOS Note Reminder', '%Note Reminder%');
INSERT INTO @tblSource VALUES ('BBOS Password Recovery', '%BBOS Password%');
INSERT INTO @tblSource VALUES ('BBOS Purchases', '%Email Purchases%');
INSERT INTO @tblSource VALUES ('BBScore Report', '%BBScore Report%');
INSERT INTO @tblSource VALUES ('Business Report Survey', '%BR Survey%');
INSERT INTO @tblSource VALUES ('Credit Monitor Report', '%Credit Monitor Report%');
INSERT INTO @tblSource VALUES ('Credit Sheet Report', 'Credit Sheet Report Event');
INSERT INTO @tblSource VALUES ('Customer Service Survey', '%CS Survey%');
INSERT INTO @tblSource VALUES ('Company Listing', '%Listing Request%');
INSERT INTO @tblSource VALUES ('Express Update Report', 'Express Update Credit Sheet Report Event');
INSERT INTO @tblSource VALUES ('Invoice', '%Generate Invoices%');
INSERT INTO @tblSource VALUES ('Listing Report Letter', '%LRL%');
INSERT INTO @tblSource VALUES ('Lumber Company Update Report', '%Credit Sheet Report%');
INSERT INTO @tblSource VALUES ('Trade Survey Request', '%TES%');

SELECT * FROM @tblSource</CommandText>
