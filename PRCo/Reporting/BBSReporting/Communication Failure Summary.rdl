  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ListingStatus">
            <Value>=Parameters!ListingStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndustryType">
            <Value>=Parameters!IndustryType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Source">
            <Value>=Parameters!Source.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prcoml_CompanyID as [BBID],
       comp_Name as [Company Name],
	   CityStateCountryShort [Listing Location],
	   CAST(capt_us as varchar(max)) [Listing Status],
       COUNT(1) as [Failure Count],
	   COUNT(CASE WHEN prcoml_MethodCode = 'FaxOut' THEN 'X' ELSE NULL END) as [Fax Failure Count],
	   COUNT(CASE WHEN prcoml_MethodCode = 'EmailOut' THEN 'X' ELSE NULL END) as [Email Failure Count],
	   COUNT(CASE WHEN prcoml_FailedTypeCode = 'BLK' THEN 'X' ELSE NULL END) as [Email Block Count],
	   COUNT(CASE WHEN prcoml_FailedTypeCode = 'BNC' THEN 'X' ELSE NULL END) as [Email Bounce Count]
  FROM PRCommunicationLog WITH (NOLOCK)
       INNER JOIN Company WITH (NOLOCK) ON prcoml_CompanyID = comp_CompanyID
	   INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
	   INNER JOIN Custom_Captions WITH (NOLOCK) ON comp_PRListingStatus = capt_code AND capt_family = 'comp_PRListingStatus'
 WHERE prcoml_Failed = 'Y'
   AND prcoml_CreatedDate BETWEEN @StartDate AND DATEADD(day, 1, @EndDate)
   AND comp_PRListingStatus IN (@ListingStatus)
   AND comp_PRIndustryType IN (@IndustryType)
AND prcoml_Source LIKE @Source
GROUP BY prcoml_CompanyID,
       comp_Name,
	   CityStateCountryShort,
	   CAST(capt_us as varchar(max))
ORDER BY COUNT(1) DESC, comp_Name</CommandText>
      </Query>
