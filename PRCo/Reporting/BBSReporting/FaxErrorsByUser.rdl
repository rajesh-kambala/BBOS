    <DataSet Name="dsFaxErrors">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserID">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DaysThrehold">
            <Value>=Parameters!DaysThrehold.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @tblErrorCodes table (
	ECode int,
    ErrorDesc varchar(50)
)


INSERT INTO @tblErrorCodes VALUES (0, 'OK')
INSERT INTO @tblErrorCodes VALUES (1, 'Fax number busy')
INSERT INTO @tblErrorCodes VALUES (2, 'Tx/Rx Error')
INSERT INTO @tblErrorCodes VALUES (3, 'Poor Quality')
INSERT INTO @tblErrorCodes VALUES (4, 'No Answer at fax #')
INSERT INTO @tblErrorCodes VALUES (5, 'Bad FCS information')
INSERT INTO @tblErrorCodes VALUES (6, 'Conversion Failed')
INSERT INTO @tblErrorCodes VALUES (7, 'FCS Create Failed')
INSERT INTO @tblErrorCodes VALUES (8, 'Scheduling Failed')
INSERT INTO @tblErrorCodes VALUES (9, 'Unknown Error Code')
INSERT INTO @tblErrorCodes VALUES (10, 'Human Answered')
INSERT INTO @tblErrorCodes VALUES (11, 'G2 Fax Machine')
INSERT INTO @tblErrorCodes VALUES (12, 'Line in use')
INSERT INTO @tblErrorCodes VALUES (13, 'Phone Line Problems')
INSERT INTO @tblErrorCodes VALUES (14, 'Bad Formtype')
INSERT INTO @tblErrorCodes VALUES (15, 'Bad Signature Code')
INSERT INTO @tblErrorCodes VALUES (16, 'Illegal Sig. Use')
INSERT INTO @tblErrorCodes VALUES (17, 'Sig needs Authoriz.')
INSERT INTO @tblErrorCodes VALUES (18, 'Fax Discarded')
INSERT INTO @tblErrorCodes VALUES (19, 'Bad phone number')
INSERT INTO @tblErrorCodes VALUES (20, 'Can''t access scans')
INSERT INTO @tblErrorCodes VALUES (21, 'Invalid Bill Code')
INSERT INTO @tblErrorCodes VALUES (22, 'Invalid Bill Code')
INSERT INTO @tblErrorCodes VALUES (23, 'OCR Error')
INSERT INTO @tblErrorCodes VALUES (24, 'Printing Error')
INSERT INTO @tblErrorCodes VALUES (25, 'No NEWLIB authoriz.')
INSERT INTO @tblErrorCodes VALUES (26, 'ViewStar import err')


SELECT UserID,
       Destination,
       prcse_CompanyId,
       prcse_FullName,
       CreationTime,
       prcoml_Source,
       prcoml_Subject,
       ErrorDesc,
       COUNT(1) As ErrorCount
  FROM ARToo.RightFax.dbo.Documents d WITH (NOLOCK)
       Inner Join ARToo.RightFax.dbo.Users u on u.handle = OwnerID
       INNER JOIN @tblErrorCodes ON ECode = ErrorCode
       LEFT OUTER JOIN PRCommunicationLog WITH (NOLOCK) ON 'BBSi' + CAST(prcoml_CommunicationLog AS VARCHAR(12)) = UniqueID
       LEFT OUTER JOIN PRCompanySearch WITH (NOLOCK) ON prcoml_CompanyID = prcse_CompanyId
 WHERE ErrorCode &gt; 0
   AND UserID=@UserID
   AND CreationTime &gt;= DATEADD(day, 0-@DaysThrehold, GETDATE())
   AND Destination IS NOT NULL
GROUP BY UserID, Destination, prcse_CompanyId, prcse_FullName, CreationTime, prcoml_Source, prcoml_Subject, ErrorDesc
HAVING COUNT(1) &gt;= 0
ORDER BY CreationTime;</CommandText>
