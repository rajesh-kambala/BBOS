        </QueryParameters>
        <CommandText>SELECT  *
FROM     
    (SELECT  
        Comm_CommunicationId AS CommID, 
        RTRIM(a.User_Logon) AS [Assigned To], 
        ISNULL(RTRIM(b.User_Logon), '') AS [Created by], 
  --      Comm_CreatedDate AS [Created Date], 
        comm_DateTime as [Due Date],
        CmLi_Comm_CompanyId AS BBID, 
        Comp_Name AS [Comp Name], 
        CityStateCountry AS Location, 
        [Listing Status] = CASE Comp_PRListingStatus 
            WHEN 'L' THEN 'Listed' 
            WHEN 'H' THEN 'Listed' 
            ELSE 'Not Listed' 
            END, 
        Communication.Comm_Note AS [Note Summary], 
        CASE 
            WHEN b.User_UserID &lt; 1 THEN 'SG' 
            WHEN b.User_UserID IS NULL THEN 'SG' 
            ELSE 'UG' END AS TaskType
    FROM 
        Communication 
        INNER JOIN Comm_Link ON Comm_CommunicationId = CmLi_Comm_CommunicationId 
        INNER JOIN Users a ON CmLi_Comm_UserId = a.User_UserId 
        LEFT OUTER JOIN Users b ON Comm_CreatedBy = b.User_UserId
        LEFT OUTER JOIN Company ON CmLi_Comm_CompanyId = Comp_CompanyId 
        LEFT OUTER JOIN vPRLocation ON Comp_PRListingCityId = prci_CityId
    WHERE  
        (a.User_UserId =  @paramCreatedBy) 
        AND (Communication.Comm_Status = 'Pending') 
        AND (Comm_Deleted IS NULL) 
        AND (CmLi_Deleted IS NULL)) AS TableA
WHERE  
        (TaskType = Case when @TaskType = 'All' then TaskType else @TaskType end)
        AND ([Due Date] between @DueFrom and @DueTo)
ORDER BY [Due Date] DESC, [Created by] DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CommID">
          <DataField>CommID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Assigned_To">
          <DataField>Assigned To</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Created_by">
          <DataField>Created by</DataField>
