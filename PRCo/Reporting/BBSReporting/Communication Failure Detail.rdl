  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Source">
            <Value>=Parameters!Source.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT comp_Name as [Company Name],
       CityStateCountryShort [Listing Location],  
       prcoml_CreatedDate as [Sent Date/Time], 
       prcoml_Subject as [Subject], 
	   prcoml_MethodCode as [Sent Method],
	   RTRIM(ISNULL(user_logon, 'System')) as [Sent By],
	   prcoml_Destination as [Address],
	   prcoml_PersonID as [Person ID],
	   dbo.ufn_FormatPerson(pers_FirstName, pers_LastName, pers_MiddleName, NULL, pers_Suffix) as [Person Name],
	   prcoml_FailedMessage as [Failure Message],
	   prcoml_FailedCategory as [Failure Category],
	   capt_US [Email Failure Type]
  FROM PRCommunicationLog WITH (NOLOCK)
       INNER JOIN Company WITH (NOLOCK) ON prcoml_CompanyID = comp_CompanyID
	   INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
	   LEFT OUTER JOIN Person WITH (NOLOCK) ON prcoml_PersonID = pers_PersonID
	   LEFT OUTER JOIN Users WITH (NOLOCK) ON prcoml_CreatedBy = user_userid
	   LEFT OUTER JOIN Custom_Captions WITH (NOLOCK) ON prcoml_FailedTypeCode = capt_code AND capt_family = 'prcoml_FailedTypeCode'
 WHERE prcoml_Failed = 'Y'
   AND prcoml_CompanyID=@CompanyID
   AND prcoml_Source LIKE @Source
   AND prcoml_CreatedDate BETWEEN @StartDate AND DATEADD(day, 1, @EndDate)
ORDER BY prcoml_CreatedDate</CommandText>
      </Query>
