  <DataSets>
    <DataSet Name="dsServerInfo">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>DECLARE @Version varchar(50)
DECLARE @VersionTbl table (
	Version sql_variant,
	val1 sql_variant,
     val2 sql_variant)



INSERT INTO @VersionTbl exec Master..sp_MSGetVersion
SELECT @Version = CONVERT(varchar(50), Version) FROM @VersionTbl

SELECT @@ServerName as ServerName, @Version As Version,
CONVERT(CHAR(25),login_time,100) as StartDate,
CONVERT(CHAR(25), DATEDIFF(DAY, login_time, GETDATE())) as 'DaysAvailable',
DB_NAME() As CurrentDB
FROM master..sysprocesses 
WHERE spid = 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ServerName">
          <DataField>ServerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Version">
          <DataField>Version</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DaysAvailable">
          <DataField>DaysAvailable</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CurrentDB">
          <DataField>CurrentDB</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
