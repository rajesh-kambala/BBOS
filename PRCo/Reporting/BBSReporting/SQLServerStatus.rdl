    <DataSet Name="dsTableSize">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SET NOCOUNT ON

IF EXISTS(SELECT * FROM TempDb.dbo.SysObjects WHERE NAME = '##Space_Used') DROP TABLE ##Space_Used 

DECLARE  @Space_Used TABLE(
	name nvarchar(128), 
	rows char(11), 
	reserved varchar(18), 
	data varchar(18), 
	index_size varchar(18), 
	unused varchar(18)
)

DECLARE @User_Table_Name varchar(200)

DECLARE User_Tables_Cursor CURSOR FOR
SELECT Name
FROM Dbo.SysObjects
WHERE XTYPE = 'U'
  AND Name NOT LIKE '%.*'

OPEN User_Tables_Cursor 

FETCH NEXT FROM User_Tables_Cursor INTO @User_Table_Name 
WHILE @@FETCH_STATUS = 0
BEGIN
	INSERT INTO @Space_Used
	EXEC sp_spaceused @User_Table_Name 

    FETCH NEXT FROM User_Tables_Cursor INTO @User_Table_Name 
END

CLOSE User_Tables_Cursor
DEALLOCATE User_Tables_Cursor


SELECT 
	TableName=LEFT(Name, 50),
	"RowCount"=CONVERT(int,Rows),
	Phys_Size_KB = CONVERT(int, LEFT(Reserved, PATINDEX('% KB', Reserved) - 1)),
    Index_Phys_Size_KB = CONVERT(int, LEFT(index_size, PATINDEX('% KB', index_size) - 1)) 
FROM @Space_Used
ORDER BY Phys_Size_KB DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="TableName">
          <DataField>TableName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RowCount">
          <DataField>RowCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Phys_Size_KB">
          <DataField>Phys_Size_KB</DataField>
