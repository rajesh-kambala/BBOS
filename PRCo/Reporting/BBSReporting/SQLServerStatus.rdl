    <DataSet Name="dsUSPExecutionCount">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@USPExecCountThreshold">
            <Value>=Parameters!USPExecCountThreshold.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DB_NAME(dbid) AS [DB_NAME], 
       OBJECT_SCHEMA_NAME(objectid,dbid) AS [SCHEMA_NAME], 
       OBJECT_NAME(objectid,dbid)AS [OBJECT_NAME], 
       SUM(usecounts) AS [Use_Count], 
       dbid, 
       objectid  
FROM sys.dm_exec_cached_plans cp
   CROSS APPLY sys.dm_exec_sql_text(cp.plan_handle)
WHERE objtype = 'Proc'
  AND OBJECT_SCHEMA_NAME(objectid,dbid) &lt;&gt; 'sys'
  AND DB_NAME(dbid)  = DB_NAME()
  AND UPPER(
-- remove white space first
            REPLACE( 
             REPLACE(
              REPLACE(
               REPLACE(
                REPLACE(
                 REPLACE(
                  REPLACE(text,'       ',' '),
                 '       ',' '),
                '      ',' '),
               '     ', ' '),
              '    ',' '),
             '   ',' '),
            '  ',' ')
           )
       LIKE '%CREATE PROC%'
GROUP BY dbid, objectid
HAVING  SUM(usecounts) &gt;=  @USPExecCountThreshold
ORDER BY Use_Count desc;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DB_NAME">
          <DataField>DB_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SCHEMA_NAME">
          <DataField>SCHEMA_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OBJECT_NAME">
          <DataField>OBJECT_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Use_Count">
          <DataField>Use_Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="dbid">
          <DataField>dbid</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="objectid">
          <DataField>objectid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
