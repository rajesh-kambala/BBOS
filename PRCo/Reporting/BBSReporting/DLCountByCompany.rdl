  <DataSets>
    <DataSet Name="BBS">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>declare @Table table
(
	BBID int,
	Tradestyle varchar(104),
	City varchar(34),
	State varchar(20),
	Country varchar(34),
	PaidLines int
)

insert into @Table (BBID, Tradestyle, City, State, Country, PaidLines)
select comp_CompanyId, 
comp_PRBookTradestyle,
prci_City as City,
prst_Abbreviation,
prcn_Country,
dbo.ufn_GetListingDLLineCount(comp_CompanyId) as PaidLines
from Company
inner join vPRLocation on prci_CityId = comp_PRListingCityId
where comp_PRListingStatus in ('L','H')
and comp_PRType = 
case when @HQBR = 'All' then comp_PRType else @HQBR end
and prcn_Country in ('USA','Canada','Puerto Rico')
--and comp_CompanyId in (102030,102175)

select * from @Table
order by PaidLines desc</CommandText>
        <QueryParameters>
          <QueryParameter Name="@HQBR">
            <Value>=Parameters!HQBR.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>BBS</DataSourceName>
      </Query>
