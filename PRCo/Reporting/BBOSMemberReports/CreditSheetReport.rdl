  <DataSets>
    <DataSet Name="dsCreditSheets">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CreditSheetIDs">
            <Value>=Parameters!CreditSheetIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prcs_PublishableDate, 
  CityStateCountryShort, 
  comp_CompanyID, 
  prcs_Tradestyle,
  dbo.ufn_GetItem(prcs_CreditSheetID, 2, 0, 34) AS ItemText
FROM PRCreditSheet
INNER JOIN Company ON prcs_CompanyID = comp_CompanyID
INNER JOIN vPRLocation on prcs_CityID = prci_CityId  
INNER JOIN custom_captions ON comp_PRIndustryType = capt_code AND capt_Family = 'comp_PRIndustryType'
WHERE prcs_CreditSheetID IN (select CAST(value AS INT) from dbo.Tokenize(@CreditSheetIDs, ','))
   AND prcs_NewListing IS NULL
ORDER BY capt_order, prcn_Country, prst_State, prci_City, prcs_Tradestyle</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="prcs_PublishableDate">
          <DataField>prcs_PublishableDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CityStateCountryShort">
          <DataField>CityStateCountryShort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="comp_CompanyID">
          <DataField>comp_CompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prcs_Tradestyle">
          <DataField>prcs_Tradestyle</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemText">
          <DataField>ItemText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCSItemsNewListings">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CreditSheetIDs">
            <Value>=Parameters!CreditSheetIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prcs_PublishableDate, 
  CityStateCountryShort, 
  comp_CompanyID, 
  prcs_Tradestyle,
  dbo.ufn_GetItem(prcs_CreditSheetID, 2, 0, 34) AS ItemText
FROM PRCreditSheet
INNER JOIN Company ON prcs_CompanyID = comp_CompanyID
INNER JOIN vPRLocation on prcs_CityID = prci_CityId  
INNER JOIN custom_captions ON comp_PRIndustryType = capt_code AND capt_Family = 'comp_PRIndustryType'
WHERE prcs_CreditSheetID IN (select CAST(value AS INT) from dbo.Tokenize(@CreditSheetIDs, ','))
   AND prcs_NewListing = 'Y'
ORDER BY capt_order, prcn_Country, prst_State, prci_City, comp_PRBookTradestyle</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="prcs_PublishableDate">
          <DataField>prcs_PublishableDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CityStateCountryShort">
          <DataField>CityStateCountryShort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="comp_CompanyID">
          <DataField>comp_CompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prcs_Tradestyle">
          <DataField>prcs_Tradestyle</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemText">
          <DataField>ItemText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
