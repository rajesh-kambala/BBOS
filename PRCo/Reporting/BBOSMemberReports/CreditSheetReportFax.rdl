    <DataSet Name="dsAd">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <CommandText>SELECT pradc_AdCampaignID,
		pradc_TargetURL,
		REPLACE(dbo.ufn_GetCustomCaptionValue('BBOS', 'URL', 'en-us') + 'Campaigns/' + pracf_FileName_Disk, 'https://', 'http://') as ImgURL,
		dbo.ufn_GetCustomCaptionValue('BBOS', 'URL', 'en-us') + 'AdClick.aspx?AdCampaignID=' + CAST(pradc_AdCampaignID as varchar(10)) + '&amp;AdAuditTrailID=0' as LinkURL
	FROM vPRAdCampaignImage
	WHERE pradc_AdCampaignTypeDigital = 'CSEU'
	AND pracf_FileTypeCode = 'DIPDF'
	AND pradc_IndustryType LIKE '%,P,%'
                   AND pradc_CreativeStatus='A'
	AND GETDATE() BETWEEN pradc_StartDate AND pradc_EndDate</CommandText>
      </Query>
      <Fields>
        <Field Name="pradc_AdCampaignID">
          <DataField>pradc_AdCampaignID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="pradc_TargetURL">
          <DataField>pradc_TargetURL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ImgURL">
          <DataField>ImgURL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LinkURL">
          <DataField>LinkURL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
