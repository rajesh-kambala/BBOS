    <DataSet Name="dsMarketing">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IndustryType">
            <Value>=Parameters!IndustryType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @CapCode varchar(20)

IF (@IndustryType =  '''L'''  ) BEGIN
     SET @CapCode = 'L'
END ELSE BEGIN
     SET @CapCode = 'P'
END

SELECT dbo.ufn_GetCustomCaptionValue('CreditSheetMarketing', @CapCode + 'Header', 'en-us') As Header,
             dbo.ufn_GetCustomCaptionValue('CreditSheetMarketing', @CapCode + 'Msg', 'en-us') As Msg</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Header">
          <DataField>Header</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Msg">
          <DataField>Msg</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
