  <DataSets>
    <DataSet Name="dsCompanies">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Culture">
            <Value>=Parameters!Culture.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyIDs">
            <Value>=Parameters!CompanyIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT comp_CompanyID AS [BBID], 
  comp_PRCorrTradestyle AS [CompanyName], 
  dbo.ufn_GetCustomCaptionValue('comp_PRIndustryType', comp_PRIndustryType, @Culture) AS Industry,
  CityStateCountryShort As [Location],  
  CountryStAbbrCity,
  dbo.ufn_FormatPhone(cp.phon_CountryCode, cp.phon_AreaCode, cp.phon_Number, cp.phon_PRExtension) As [CompanyPhoneNumber],         
  CASE comp_PRLocalSource WHEN 'Y' THEN NULL ELSE dbo.ufn_FormatPhone(cf.phon_CountryCode, cf.phon_AreaCode, cf.phon_Number, cf.phon_PRExtension) END As  [CompanyFaxNumber],         

  dbo.ufn_FormatPhone(pp.phon_CountryCode, pp.phon_AreaCode, pp.phon_Number, pp.phon_PRExtension) As [PersonPhoneNumber],
  CASE comp_PRLocalSource WHEN 'Y' THEN NULL ELSE dbo.ufn_FormatPhone(pf.phon_CountryCode, pf.phon_AreaCode, pf.phon_Number, pf.phon_PRExtension) END As  [PersonFaxNumber],         
  prra_RatingLine AS Rating,
  RTRIM(ce.emai_EmailAddress) AS [Email], 	
  RTRIM(pe.emai_EmailAddress) AS [PersonEmail], 	
  dbo.ufn_FormatPerson(Pers_FirstName, Pers_LastName, Pers_MiddleName, pers_PRNickname1, Pers_Suffix) AS HeadExecutive
FROM Company WITH (NOLOCK)
INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
LEFT OUTER JOIN vPRCompanyRating ON comp_CompanyID = prra_CompanyID AND prra_Current='Y'
 LEFT OUTER JOIN Person_Link WITH (NOLOCK) ON comp_CompanyID = PeLi_CompanyID 
    AND PeLi_PREBBPublish = 'Y' 
    AND PeLi_PRRole LIKE '%,HE,%'
    AND PeLi_PRStatus = 1
   LEFT OUTER JOIN Person WITH (NOLOCK) ON PeLi_PersonID = pers_PersonID 
    LEFT OUTER JOIN vPRPersonPhone AS pp WITH (NOLOCK) ON comp_CompanyID = pp.phon_CompanyID AND peli_PersonID = pp.plink_RecordID AND pp.Phon_PRPreferredPublished = 'Y' AND pp.phon_PRIsPhone = 'Y'     
    LEFT OUTER JOIN vPRPersonPhone AS pf WITH (NOLOCK) ON comp_CompanyID = pf.phon_CompanyID AND peli_PersonID = pf.plink_RecordID  AND pf.Phon_PRPreferredPublished = 'Y' AND pf.Phon_PRIsFax = 'Y'
    LEFT OUTER JOIN vPRCompanyPhone AS cp WITH (NOLOCK) ON comp_CompanyID = cp.plink_RecordID AND cp.Phon_PRPreferredPublished = 'Y' AND cp.phon_PRIsPhone = 'Y'
    LEFT OUTER JOIN vPRCompanyPhone AS cf WITH (NOLOCK) ON comp_CompanyID = cf.plink_RecordID  AND cf.Phon_PRPreferredPublished = 'Y' AND cf.Phon_PRIsFax = 'Y'
	LEFT OUTER JOIN vPersonEmail AS pe WITH (NOLOCK) ON comp_CompanyID = pe.emai_CompanyID AND peli_PersonID = pe.ELink_RecordID  AND pe.elink_Type='E' AND pe.emai_PRPreferredPublished='Y'   
	LEFT OUTER JOIN vCompanyEmail AS ce WITH (NOLOCK) ON comp_CompanyID = ce.ELink_RecordID AND ce.elink_Type='E' AND ce.emai_PRPreferredPublished='Y'   
WHERE comp_CompanyID IN (select CAST(value AS INT) from dbo.Tokenize(@CompanyIDs, ','))
ORDER BY comp_PRCorrTradestyle</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
