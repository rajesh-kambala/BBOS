    <DataSet Name="dsSecurityLevels">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <CommandText>SELECT CAST(RTRIM(capt_code) AS Int) As SecurityLevel, capt_us
  FROM custom_captions
WHERE capt_family = 'prwu_AccessLevel'
ORDER BY capt_order</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SecurityLevel">
          <DataField>SecurityLevel</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="capt_us">
          <DataField>capt_us</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCulture">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <CommandText>SELECT RTRIM(capt_code) as capt_code, capt_us
 FROM Custom_Captions
WHERE capt_family = 'prwu_Culture'
ORDER BY capt_order</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
