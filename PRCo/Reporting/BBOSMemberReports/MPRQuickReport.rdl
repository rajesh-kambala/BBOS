  <Code>Public Function DisplayCompanyPersonInfo (szCompanyValue As String, _
  szPersonValue As String, iLevel As Integer)  As String
    If iLevel &gt;= 400 Then
        If Trim(szPersonValue) &lt;&gt; "" Then
            Return szPersonValue
        Else
            Return szCompanyValue
        End If
    Else
        Return szCompanyValue
    End If
End Function</Code>
