      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <CommandText>SELECT c.comp_CompanyId, 
  [Company Name], 
  [Mailing Address Line 1],
  [Mailing Address Line 2],
  [Mailing Address Line 3],
  [Mailing Address Line 4],
  [Mailing Address Line 5],
  [Mailing City],
  [Mailing State], 
  [Mailing Country],
  [Mailing Postal Code],
  [Physical Address Line 1],
  [Physical Address Line 2],
  [Physical Address Line 3],
  [Physical Address Line 4],
  [Physical Address Line 5],
  [Physical City],
  [Physical State],
  [Physical Country],
  [Physical Postal Code],
  [Phone Number],
  [Fax Number],
  [E-mail Address],
  [Web Site URL], 
  [Executive Contact Name],
  [Classifications],
  [Commodities],
 dbo.ufn_GetCustomCaptionValue('prcp_Volume',   [Annual Volume Figure], 'en-us')  As [Annual Volume Figure]
FROM vPRMarketingList ml
INNER JOIN Company c ON c.comp_CompanyID = ml.comp_CompanyID
WHERE ml.comp_CompanyID IN (select CAST(value AS INT) from dbo.Tokenize(@CompanyIDs, ','))
ORDER BY [Company Name]</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CompanyIDs">
            <Value>=Parameters!CompanyIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
