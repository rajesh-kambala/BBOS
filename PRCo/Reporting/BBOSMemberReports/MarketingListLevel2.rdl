  <Author>Travant Solutions, Inc.</Author>
  <Code>Public Function FormatAddress(szAddressLine1 As String, szAddressLine2 As String, _
  szAddressLine3 As String, szAddressLine4 As String, szAddressLine5 As String, _
  szCity As String, szState As String, szCountry As String, szPostalCode As String) As String

    Dim szAddress As String

    szAddress = szAddressLine1
    If Trim(szAddressLine2) &lt;&gt; "" Then
        szAddress &amp;= vbNewLine &amp; szAddressLine2
    End If
    If Trim(szAddressLine3) &lt;&gt; "" Then
        szAddress &amp;= vbNewLine &amp; szAddressLine3
    End If
    If Trim(szAddressLine4) &lt;&gt; "" Then
        szAddress &amp;= vbNewLine &amp; szAddressLine4
    End If
    If Trim(szAddressLine5) &lt;&gt; "" Then
        szAddress &amp;= vbNewLine &amp; szAddressLine5
    End If

    szAddress &amp;= vbNewLine &amp; szCity
    If Trim(szCity) &lt;&gt; "" Then
        szAddress &amp;= ", "
    End If
    szAddress &amp;= szState
    szAddress &amp;= "  " &amp; szPostalCode

    If Trim(szCountry) &lt;&gt; "USA" Then
        szAddress &amp;= vbNewLine &amp; szCountry
    End If

    Return szAddress

End Function





</Code>
