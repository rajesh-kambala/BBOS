  <DataSets>
    <DataSet Name="dsAdCampaign">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AdvertisingCampaignID">
            <Value>=Parameters!AdvertisingCampaignID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT pradc_Name, 
            CASE WHEN pradc_AdCampaignType = 'PUB' THEN dbo.ufn_GetAdPublicationViewCount(pradc_AdCampaignID) ELSE pradc_ImpressionCount END As pradc_ImpressionCount,
            pradc_ClickCount
FROM PRAdCampaign
WHERE pradc_AdCampaignID = @AdvertisingCampaignID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="pradc_Name">
          <DataField>pradc_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="pradc_ImpressionCount">
          <DataField>pradc_ImpressionCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="pradc_ClickCount">
          <DataField>pradc_ClickCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
