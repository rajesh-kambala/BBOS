      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <CommandText>DECLARE @RowCount int = 12

DECLARE @Count int = 0
DECLARE @MyTable table (BlankRow varchar(10))

WHILE @Count &lt; @RowCount BEGIN
	INSERT INTO @MyTable VALUES (' ')
	SET @Count = @Count + 1
END

SELECT * FROM @MyTable</CommandText>
