    <DataSet Name="dsCompanies">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyIDs">
            <Value>=Parameters!CompanyIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ReportTable TABLE  (
  BBID int, 
  CompanyName varchar(104), 
  Location varchar(104),
  CountryStAbbrCity varchar(104),
  CurrentRatingLine varchar(100), 
  DateReported datetime,
  PreviousRatingLine varchar(100),
  DateLastFinancialStatement datetime
)

INSERT INTO @ReportTable (BBID, CompanyName, Location, CountryStAbbrCity, CurrentRatingLine, DateReported, DateLastFinancialStatement)
SELECT comp_CompanyID, 
  comp_PRBookTradestyle, 
  CityStateCountryShort,
  CountryStAbbrCity,
  prra_RatingLine,
  prra_Date, 
  comp_PRFinancialStatementDate
FROM Company
INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
LEFT OUTER JOIN vPRCompanyRating ON comp_CompanyID = prra_CompanyID AND prra_Current='Y'
WHERE comp_CompanyID IN (select CAST(value AS INT) from dbo.Tokenize(@CompanyIDs, ','))
ORDER BY comp_PRBookTradestyle

UPDATE @ReportTable
SET PreviousRatingLine = prra_RatingLine
FROM vPRCompanyRating
WHERE BBID = prra_CompanyID
AND prra_RatingID = (SELECT MAX(prra_RatingId) FROM vPRCompanyRating WHERE prra_Date &lt; DateReported AND prra_CompanyID= BBID)

SELECT * FROM @ReportTable ORDER BY CompanyName</CommandText>
