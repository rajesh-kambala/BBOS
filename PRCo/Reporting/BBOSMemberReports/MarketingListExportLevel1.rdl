  <DataSets>
    <DataSet Name="dsCompanies">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT c.comp_CompanyId AS [BBID], 
  [Company Name] As [CompanyName], 
  [Mailing Address Line 1] As [MailingAddressLine1],
  [Mailing Address Line 2] As [MailingAddressLine2],
  [Mailing Address Line 3] As [MailingAddressLine3],
  [Mailing Address Line 4] As [MailingAddressLine4],
  [Mailing Address Line 5] As [MailingAddressLine5],
  [Mailing City] As [MailingCity],
  [Mailing State] As [MailingState], 
  [Mailing Country] As [MailingCountry],
  [Mailing Postal Code] As [MailingPostalCode],
  [Physical Address Line 1] As [PhysicalAddressLine1],
  [Physical Address Line 2] As [PhysicalAddressLine2],
  [Physical Address Line 3] As [PhyscialAddressLine3],
  [Physical Address Line 4] As [PhysicalAddressLine4],
  [Physical Address Line 5] As [PhysicalAddressLine5],
  [Physical City] As [PhysicalCity],
  [Physical State] As [PhysicalState],
  [Physical Country] As [PhysicalCountry],
  [Physical Postal Code] As [PhysicalPostalCode],
  [Phone Number] As [PhoneNumber],
  [Fax Number] As [FAXNumber],
  [E-mail Address] As [EmailAddress]

FROM vPRMarketingList ml
INNER JOIN Company c ON c.comp_CompanyID = ml.comp_CompanyID
WHERE ml.comp_CompanyID IN (select CAST(value AS INT) from dbo.Tokenize(@CompanyIDs, ','))
ORDER BY [Company Name]</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CompanyIDs">
            <Value>=Parameters!CompanyIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>BBOS</DataSourceName>
      </Query>
