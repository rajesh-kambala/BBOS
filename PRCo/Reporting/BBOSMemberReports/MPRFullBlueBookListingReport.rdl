  <DataSets>
    <DataSet Name="dsCompanies">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyIDs">
            <Value>=Parameters!CompanyIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT dbo.ufn_GetListingCache(comp_CompanyID, 2) AS Listing,
  dbo.ufn_GetRatingDefinitions(prra_RatingID, 2, 34) As RatingDefinition,
  comp_CompanyID AS BBID, 
  prra_RatingID, 
  CityStateCountryShort,
  CountryStAbbrCity,
  comp_PRCorrTradestyle AS CompanyName
FROM Company 
INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
LEFT OUTER JOIN PRRating ON comp_PRHQID = prra_CompanyID AND prra_Current='Y'
WHERE comp_CompanyID IN (select CAST(value AS INT) from dbo.Tokenize(@CompanyIDs, ','))
AND comp_PRLocalSource IS NULL
ORDER BY prcn_Country, prst_State, prci_City, comp_PRBookTradestyle</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Listing">
          <DataField>Listing</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RatingDefinition">
          <DataField>RatingDefinition</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BBID">
          <DataField>BBID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prra_RatingID">
