  <Code>Public Shared IndustryType as String
Public Shared IsMember as String
Public Shared BBID as String
Public Shared HQID as String
Public Shared CompanyName as String
Public Shared Location as String
Public Shared FooterURL as String
Public Shared FaxNumber as String

Public Shared Gus as String
Public Function SetGus  (ByVal var as Integer)
               Gus  = CStr(var)
               return Gus
End Function

Public Function GetHQID()
	return HQID
END Function

Public Function SetHQID  (ByVal var as Integer)
               HQID = CStr(var)
End Function

Public Function SetCompanyName (ByVal var as String)
	CompanyName= var
End Function

Public Function SetLocation (ByVal City as String, ByVal State as String, ByVal Country as String)
	Location = City + ", " + State 

	if (Country  &lt;&gt; "USA") then
		Location += " " + Country
	end if
End Function

Public Function SetIndustryType (ByVal var as String)
	IndustryType = var

	if (IndustryType = "L") then
		FooterURL = "www.lumberbluebook.com"
                                     FaxNumber = "630 344-0365"
	else
		FooterURL = "www.producebluebook.com"
                                     FaxNumber = "630 344-0388"
	end if
	
End Function

Public Function SetIsMember (ByVal var as String)
	IsMember = var
	if (IsMember = "Y") then
		IsMember = "M"
                   else
		IsMember = "N"
	end if
End Function

Public Function SetBBID (ByVal var as Integer)
	BBID = CStr(var)
End Function


Public Function FormatAddress(ByVal address1 as String, ByVal address2 as String, ByVal address3 as String, ByVal address4 as String) 

	Dim address as String
	address = address1

	if (address2 &lt;&gt; nothing) then
		address += Environment.NewLine + address2
	end if	

	if (address3 &lt;&gt; nothing) then
		address += Environment.NewLine + address3
	end if	

	if (address4 &lt;&gt; nothing) then
		address += Environment.NewLine + address4
	end if

	return address
End Function


Public Function Bullet1Visibility(ByVal industryType As String, ByVal clMessage As String)

	if IndustryType = "S" or IndustryType = "L" then
		return true
	end if

	if (clMessage = "Request Update") then
		return false
	end if

	return true
End Function


Public Function Bullet2Visibility(ByVal industryType As String, ByVal clMessage As String)

	if IndustryType = "S" or IndustryType = "L" then
		return true
	end if

	if (clMessage = "Request First Time") then
		return false
	end if

	return true
End Function


Public Function GetAttention(ByVal attention)

              if (attention is nothing) then
                           return ""
              end if

	if (attention.ToLower().StartsWith("attn: ")) then
                   	return attention.Substring(6, attention.Length - 6)
	end if

	return attention  
End Function


Public Function LumberVolumeVisibility(ByVal industryType As String, ByVal level1Classifications As String, ByVal volumeMessage as String)

  if industryType &lt;&gt; "L" then
	  return true
  end if

  if (InStr(level1Classifications, "2181") &gt; 0) OR (InStr(level1Classifications, "2184") &gt; 0) then
    if (volumeMessage = "Request Volume") then
    return false
    end if

    return true
    end if

    return true
    End Function

    Public Shared PreviousPageName As System.Collections.Generic.List(Of Integer)

Public Shared pageNum as Integer
Public Shared Function setPageNum(ByVal x as Integer) as Integer
	pageNum = x
  return pageNum
End Function

    Public Shared Function getCurrent(ByVal Item As Integer) As String
    If PreviousPageName Is Nothing Then
      PreviousPageName = New System.Collections.Generic.List(Of Integer)
    End If

    If Not PreviousPageName.Contains(Item) Then
      PreviousPageName.Add(Item)
    End If
    Return Item
    End Function

    Public Shared Function getPrevious(ByVal Item As Integer) As String
    if Item=1 then return ""
    Return PreviousPageName(Item-1).ToString()
    End Function

    Public Function getPageCount() As Integer
         Return Me.Report.Globals!PageNumber    
    End Function

