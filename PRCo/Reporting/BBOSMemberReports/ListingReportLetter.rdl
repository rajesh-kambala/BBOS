    <DataSet Name="dsBranches">
      <Query>
        <DataSourceName>BBOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT comp_CompanyID
  FROM Company WITH(NOLOCK) 
       INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID 
 WHERE comp_PRType = 'B' 
   AND comp_PRListingStatus IN ('L', 'H', 'LUV') 
   AND comp_PRHQID = @CompanyID 
ORDER BY prcn_Country, prst_State, prci_City, comp_PRBookTradeStyle</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="comp_CompanyID">
          <DataField>comp_CompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
