  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT comp_CompanyID as [BBID],
       comp_Name as [Company Name],
	   CityStateCountryShort as [Listing Location],
		dbo.ufn_GetRecentBluePrintsEditions(Comp_CompanyId, 6) Editions
FROM PRCompanyInfoProfile 
    INNER JOIN Company WITH (NOLOCK) ON prc5_CompanyId = comp_CompanyID
	  INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
WHERE 
		prc5_PRARYearRoundBPAdvertiser = 'Y'
ORDER BY comp_Name</CommandText>
      </Query>
      <Fields>
        <Field Name="BBID">
          <DataField>BBID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Company_Name">
          <DataField>Company Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Listing_Location">
          <DataField>Listing Location</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Editions">
          <DataField>Editions</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
