        <QueryParameters>
          <QueryParameter Name="@ListedFilter">
            <Value>=Parameters!ListedFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @MaxDRCNumber int
declare @idx int
declare @Tbl table (DRCNumber varchar(20))

--select @MaxDRCNumber = max(cast(prdr_LicenseNumber as int)) from PRDRCLicense
select @MaxDRCNumber = max(prdr_LicenseNumber) from PRDRCLicense

set @idx = 1
while (@idx &lt;= @MaxDRCNumber) begin
	insert into @Tbl (DRCNumber)
	values (@idx)
	set @idx = @idx + 1	
end

declare @Tbl2 table (
	DRCNumber varchar(20),
	BBID int,
	Tradestyle varchar(104),
	ListingStatus varchar(2),
	Publish varchar(1),
	Listed varchar(1)
)

insert into @Tbl2 (DRCNumber, BBID, Tradestyle, ListingStatus, Publish, Listed)
select DRCNumber,
 prdr_CompanyID,
 comp_PRBookTradestyle,
 comp_PRListingStatus,
 prdr_Publish,
 case 
	when comp_PRListingStatus in ('L','H') and prdr_Publish = 'Y' then 'Y'
                when prdr_CompanyId is null then 'U'
	else 'N'
 end
from @Tbl
left outer join PRDRCLicense on DRCNumber = prdr_LicenseNumber
left outer join Company on comp_CompanyId = prdr_CompanyId
order by DRCNumber

select * from @Tbl2
where Listed = case	
	when @ListedFilter = 'A' then Listed 
	else @ListedFilter
end</CommandText>
