          <DataField>BBID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Tradestyle">
          <DataField>Tradestyle</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Lines">
          <DataField>Lines</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
