  <DataSets>
    <DataSet Name="BBS">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MaxProduceBodyLines">
            <Value>=Parameters!MaxProduceBodyLines.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MaxTransportationBodyLines">
            <Value>=Parameters!MaxTransportationBodyLines.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MaxSupplyBodyLines">
            <Value>=Parameters!MaxSupplyBodyLines.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- Query for listings whose body lines exceed max allowed
SELECT comp_CompanyId as BBID, 
       comp_PRBookTradestyle as Tradestyle,
       dbo.ufn_GetListingBodyLineCount(comp_CompanyId) as Lines
  FROM Company WITH (NOLOCK)
 WHERE comp_PRListingStatus in ('L','H')
   AND comp_PRLocalSource IS NULL
  AND ((comp_PRIndustryType = 'P' and dbo.ufn_GetListingBodyLineCount(comp_CompanyId) &gt; @MaxProduceBodyLines)
       OR (comp_PRIndustryType = 'T' and dbo.ufn_GetListingBodyLineCount(comp_CompanyId) &gt; @MaxTransportationBodyLines)
       OR (comp_PRIndustryType = 'S' and dbo.ufn_GetListingBodyLineCount(comp_CompanyId) &gt; @MaxSupplyBodyLines))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
