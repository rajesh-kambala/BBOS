  <DataSets>
    <DataSet Name="dsListings">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Text">
            <Value>=Parameters!Text.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ListingStatus">
            <Value>=Parameters!ListingStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndustryType">
            <Value>=Parameters!IndustryType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SortOrder">
            <Value>=Parameters!SortOrder.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Declare @Find varchar(1000)
SET @Find = '%' + @Text + '%'


SELECT comp_CompanyID, comp_Name, prlst_Listing as Listing
  FROM PRListing WITH (NOLOCK)
       INNER JOIN Company WITH (NOLOCK) ON prlst_CompanyID = comp_CompanyID
 WHERE prlst_Listing Like @Find
  AND comp_PRListingStatus IN (@ListingStatus)
  AND comp_PRIndustryType IN (@IndustryType)
ORDER BY CASE @SortOrder WHEN '1' THEN comp_Name WHEN '2' THEN CAST(comp_CompanyID as varchar(10)) END;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="comp_CompanyID">
          <DataField>comp_CompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="comp_Name">
          <DataField>comp_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Listing">
          <DataField>Listing</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsListingStatus">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT RTRIM(capt_code) as capt_code, capt_us 
  FROM custom_captions 
WHERE capt_family = 'comp_PRListingStatus' 
ORDER BY capt_order;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="capt_code">
          <DataField>capt_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="capt_us">
          <DataField>capt_us</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsIndustryType">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT RTRIM(capt_code) as capt_code, capt_us 
  FROM custom_captions 
WHERE capt_family = 'comp_PRIndustryType' 
ORDER BY capt_order;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="capt_code">
          <DataField>capt_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="capt_us">
          <DataField>capt_us</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
