  <DataSets>
    <DataSet Name="dsCountries">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT prcn_CountryID, CASE prcn_Country WHEN '' THEN '&lt;NULL&gt;' ELSE prcn_Country END As prcn_Country
  FROM PRCountry
ORDER BY prcn_BookOrder</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="prcn_CountryID">
          <DataField>prcn_CountryID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prcn_Country">
          <DataField>prcn_Country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsStates">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CountryID">
            <Value>=Parameters!CountryID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prst_CountryID, prst_StateID, CASE WHEN prst_State = '' THEN '&lt;NULL&gt;' WHEN prst_State IS NULL THEN '--None--' ELSE prst_State END As prst_State
  FROM PRState
WHERE prst_CountryID=@CountryID
ORDER BY prst_BookOrder</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="prst_CountryID">
          <DataField>prst_CountryID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prst_StateID">
          <DataField>prst_StateID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prst_State">
          <DataField>prst_State</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCities">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StateID">
            <Value>=Parameters!StateID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prci_StateID, prci_CityID, prci_City
  FROM PRCity
WHERE prci_StateID=@StateID
ORDER BY prci_City</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="prci_StateID">
          <DataField>prci_StateID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prci_CityID">
          <DataField>prci_CityID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prci_City">
          <DataField>prci_City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCompanies">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CityID">
            <Value>=Parameters!CityID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT comp_CompanyID, 
       comp_Name, 
	   ll.CityStateCountryShort,
                      ll.CountryStAbbrCity,
	   comp_PRIndustryType,
	   CAST(capt_us as varchar(500)) as ListingStatus
  FROM Company WITH (NOLOCK)
       INNER JOIN vPRLocation ll ON comp_PRListingCityID = ll.prci_CityID
	   INNER JOIN Custom_Captions ls ON comp_PRListingStatus = ls.capt_code and ls.capt_family = 'comp_PRListingStatus'
       LEFT OUTER JOIN vPRAddress ON comp_CompanyID = adli_CompanyID
 WHERE (comp_PRListingCityID=@CityID
        OR addr_PRCityID=@CityID)
ORDER BY comp_Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="comp_CompanyID">
          <DataField>comp_CompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="comp_Name">
          <DataField>comp_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CityStateCountryShort">
          <DataField>CityStateCountryShort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CountryStAbbrCity">
          <DataField>CountryStAbbrCity</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="comp_PRIndustryType">
          <DataField>comp_PRIndustryType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ListingStatus">
          <DataField>ListingStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
