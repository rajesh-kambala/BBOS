  <DataSets>
    <DataSet Name="dsCompanies">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IncludeOnlineOnlyCompanies">
            <Value>=Parameters!IncludeOnlineOnlyCompanies.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Industry">
            <Value>=Parameters!Industry.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT comp_CompanyID as BBID, 
       comp_Name as [Company Name], 
       comp_PRType as [H/B], 
       prci_City as [City], 
       prst_State as [State],
       prcn_Country as [Country],
       comp_PRLogo [Logo File],
       [LogoStatus] = Case 
			When comp_PRPublishLogo = 'Y' then 'Published'
			When comp_PRLogo is null then 'Missing Logo Path'
			else 'Missing Logo Image'
		end,
       comp_PRIndustryType,
	   --dbo.ufn_GetCustomCaptionValue('PIKSUtils', 'LogoURL', 'en-us') + comp_PRLogo + '&amp;Width=125&amp;Height=125' AS comp_PRLogo,
	   'http://apps.bluebookservices.com/BBSUtils/GetLogo.aspx?LogoFile=' + comp_PRLogo + '&amp;Width=125&amp;Height=125' AS comp_PRLogo,
	   dbo.ufn_GetCustomCaptionValue('PIKSUtils', 'LogoURL', 'en-us') + comp_PRLogo  AS comp_PRLogoURL 
		 ,comp_PROnlineOnly
  FROM Company 
       INNER JOIN vPRLocation on prci_CityID = comp_PRListingCityID 
       INNER JOIN PRService on prse_CompanyID = comp_CompanyID 
 WHERE prse_ServiceCode IN ('STANDARD', 'PREMIUM', 'ENTERPRISE', 'LOGO', 'LMBLOGO', 'LMBLOGO-ADD', 'LOGO-ADD')
   AND comp_PRListingStatus IN ('L', 'H', 'LUV')
   AND
	 (
			(@IncludeOnlineOnlyCompanies=1)
			OR
			(@IncludeOnlineOnlyCompanies=0 AND comp_PROnlineOnly IS NULL)
	 )
   AND comp_PRIndustryType in (@Industry)
ORDER BY comp_PRIndustryType, prcn_Country, prst_State, prci_City, comp_Name, Comp_CompanyID;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
