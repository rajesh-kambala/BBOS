  <DataSets>
    <DataSet Name="dsType">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT RTRIM(capt_code) As capt_Code, capt_us FROM Custom_Captions WITH (NOLOCK) where capt_family = 'comp_PRType' ORDER BY capt_Order</CommandText>
      </Query>
      <Fields>
        <Field Name="capt_Code">
          <DataField>capt_Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="capt_us">
          <DataField>capt_us</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsIndustryType">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT RTRIM(capt_code) As capt_Code, capt_us FROM Custom_Captions WITH (NOLOCK) where capt_family = 'comp_PRIndustryType' ORDER BY capt_Order</CommandText>
      </Query>
      <Fields>
        <Field Name="capt_Code">
          <DataField>capt_Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="capt_us">
