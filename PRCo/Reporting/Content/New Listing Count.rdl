        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ThroughDate">
            <Value>=Parameters!ThroughDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TYPE">
            <Value>=Parameters!Type.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CreatedBy">
            <Value>=Parameters!CreatedBy.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@IndustryType">
            <Value>=Parameters!IndustryType.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	u.User_Logon [User Initials],
	cc.Capt_US [Industry Type],
	cc2.Capt_US [Type],
	cc3.Capt_US [Status],
	c.comp_CompanyID [BBID],
	c.comp_Name [Company Name],
	vprl.prci_city [City],
	vprl.prst_State [State],
	vprl.prcn_Country [Country],
	c.comp_PRListedDate
FROM Company c WITH (NOLOCK) 
  INNER JOIN vPRLocation vprl WITH (NOLOCK) ON c.comp_PRListingCityID = vprl.prci_CityID
	INNER JOIN PRTransaction prt ON prt.prtx_CompanyId = c.comp_CompanyId
	INNER JOIN PRTransactionDetail prtd ON prtd.prtd_TransactionId = prt.prtx_TransactionID AND prtd.prtd_NewValue = 'Listed' AND CAST(prtd.prtd_CreatedDate as Date) = CAST(c.comp_PRListedDate AS Date)
	INNER JOIN Users u ON u.User_UserId = prtd.prtd_UpdatedBy
	INNER JOIN Custom_Captions cc WITH (NOLOCK) ON cc.Capt_Family='comp_PRIndustryType' AND cc.Capt_Code = c.comp_PRIndustryType
	INNER JOIN Custom_Captions cc2 WITH (NOLOCK) ON cc2.Capt_Family='comp_PRType' AND cc2.Capt_Code = c.comp_PRType
	INNER JOIN Custom_Captions cc3 WITH (NOLOCK) ON cc3.Capt_Family='comp_PRListingStatus' AND cc3.Capt_Code = c.comp_PRListingStatus
WHERE
	c.comp_PRListedDate BETWEEN @FromDate AND @ThroughDate		
	AND c.comp_PRType IN (@TYPE)
	AND prt.prtx_CreatedBy IN (@CreatedBy)
	AND c.comp_PRIndustryType IN (@IndustryType)
ORDER BY u.user_logon, c.comp_name, c.comp_companyid</CommandText>
      </Query>
      <Fields>
        <Field Name="User_Initials">
          <DataField>User Initials</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Industry_Type">
          <DataField>Industry Type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
