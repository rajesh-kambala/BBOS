    <DataSet Name="dsContentManagers">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT DISTINCT user_userID, User_Logon
  FROM Users
       INNER JOIN Channel_Link ON User_UserId = ChLi_User_Id
 WHERE (ChLi_Channel_Id = 4 OR User_PrimaryChannelId = 4)
   AND User_Disabled IS NULL
ORDER BY user_logon  </CommandText>
      </Query>
      <Fields>
        <Field Name="user_userID">
          <DataField>user_userID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="User_Logon">
          <DataField>User_Logon</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
