          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>Select
	BBID = comp_CompanyID
	,[Name] = comp_Name
	,[Location] = CityStateCountryShort
	,[HQ BBID] = comp_PRHQID
	,[Industry] = comp_PRIndustryType
	,[Status] = case comp_PRlistingStatus
		when 'L' then 'Listed'
		when 'H' then 'Hold'
		when 'LUV' then 'LVP'
		else 'Not Listed' end
From Company with (nolock)
inner join vPRLocation with (nolock) on prci_CityID = comp_PRListingCityID
where comp_PRListingStatus in ('L','H','Luv')
and comp_PRHQID not in (Select comp_CompanyID from Company with (nolock) where comp_PRListingStatus in ('L','H','LUV'))
order by comp_PRHQID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
