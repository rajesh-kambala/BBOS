  <DataSets>
    <DataSet Name="dsARActivity">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ListingStatus">
            <Value>=Parameters!ListingStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UniqueSubmittersThreshold">
            <Value>=Parameters!UniqueSubmittersThreshold.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ComputedEndDate datetime
SET @ComputedEndDate = DATEADD(day, 1, @ToDate)

DECLARE @RecentAR table (
	ReportingCompanyID int,
    SubjectCompanyID int,
    MostRecentARSubmissionDate datetime)

	INSERT INTO @RecentAR
		SELECT praa_CompanyID As ReportingCompanyID,
			   CASE WHEN praad_ManualCompanyId IS NOT NULL THEN praad_ManualCompanyId ELSE prar_PRCoCompanyId END As SubjectCompanyID,
			   MAX(praa_Date) As MostRecentARSubmissionDate
		  FROM PRARAgingDetail 
			   INNER JOIN PRARAging ON praa_ARAgingId = praad_ARAgingId 
			   LEFT OUTER JOIN PRARTranslation ON praad_ARCustomerId = prar_CustomerNumber AND prar_CompanyId = praa_CompanyId 
			   INNER JOIN Company ON CASE WHEN praad_ManualCompanyId IS NOT NULL THEN praad_ManualCompanyId ELSE prar_PRCoCompanyId END = comp_CompanyId 
		 WHERE comp_PRIndustryType = 'L'
                                      AND comp_PRListingStatus IN (@ListingStatus)
           AND praa_Date BETWEEN @FromDate AND @ComputedEndDate
		GROUP BY praa_CompanyID, CASE WHEN praad_ManualCompanyId IS NOT NULL THEN praad_ManualCompanyId ELSE prar_PRCoCompanyId END


--SELECT * FROM @RecentAR




SELECT praad_SubjectCompanyID,
       Company .comp_Name,
       CityStateCountryShort,
       comp_PRListingStatus,
       SubmitterCount,
       TotalAmountCurrent,
       TotalAmount1to30,
       TotalAmount31to60,
       TotalAmount61to90,
       TotalAmount91Plus,
  dbo.ufn_Divide(TotalAmountCurrent, (TotalAmount1to30 + TotalAmount31to60 + TotalAmount61to90 + TotalAmount91Plus + TotalAmountCurrent)) As PercAmountCurrent,
  dbo.ufn_Divide(TotalAmount1to30, (TotalAmount1to30 + TotalAmount31to60 + TotalAmount61to90 + TotalAmount91Plus + TotalAmountCurrent)) As PercAmount1to30,
  dbo.ufn_Divide(TotalAmount31to60, (TotalAmount1to30 + TotalAmount31to60 + TotalAmount61to90 + TotalAmount91Plus + TotalAmountCurrent)) As PercAmount31to60,
  dbo.ufn_Divide(TotalAmount61to90, (TotalAmount1to30 + TotalAmount31to60 + TotalAmount61to90 + TotalAmount91Plus + TotalAmountCurrent)) As PercAmount61to90,
  dbo.ufn_Divide(TotalAmount91Plus, (TotalAmount1to30 + TotalAmount31to60 + TotalAmount61to90 + TotalAmount91Plus + TotalAmountCurrent)) As PercAmount91Plus,
  prra_RatingLine
FROM (
	SELECT praad_SubjectCompanyID,
           COUNT(DISTINCT praad_ReportingCompanyID) As SubmitterCount,
           SUM(praad_AmountCurrent) As TotalAmountCurrent, 
           SUM(praad_Amount1to30) As TotalAmount1to30, 
		   SUM(praad_Amount31to60) As TotalAmount31to60, 
		   SUM(praad_Amount61to90) As TotalAmount61to90, 
		   SUM(praad_Amount91Plus) As TotalAmount91Plus
	  FROM vPRARAgingDetailOn a
           INNER JOIN @RecentAR ON praad_SubjectCompanyID = SubjectCompanyID
                                AND praad_ReportingCompanyID = ReportingCompanyID
                                AND praa_Date = MostRecentARSubmissionDate
	  GROUP BY praad_SubjectCompanyID) T1
	 INNER JOIN Company WITH (NOLOCK) ON praad_SubjectCompanyID = comp_CompanyID
	 INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
                 LEFT OUTER JOIN vPRCompanyRating ON praad_SubjectCompanyID = prra_CompanyID and prra_Current = 'Y'
	WHERE SubmitterCount &gt;= @UniqueSubmittersThreshold
ORDER BY PercAmountCurrent</CommandText>
