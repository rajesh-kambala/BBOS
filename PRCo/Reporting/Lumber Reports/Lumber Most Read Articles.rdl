  <DataSources>
    <DataSource Name="BBS">
      <DataSourceReference>BBS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>deb4d323-25bc-4e56-be4b-7b3173771a50</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="dsDetails">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TopCount">
            <Value>=Parameters!TopCount.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@PublishStart">
            <Value>=Parameters!PublishStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PublishEnd">
            <Value>=Parameters!PublishEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP (CAST(@TopCount AS Integer)) p.post_title, p.guid, CAST(pm.meta_value AS INT) AS view_count
FROM WordPressLumber.dbo.wp_posts AS p
	INNER JOIN WordPressLumber.dbo.wp_postmeta AS pm ON pm.post_id = p.ID AND pm.meta_key = 'wpb_post_views_count'
WHERE p.post_status = 'publish'
   AND p.post_type  = 'post'
   AND p.post_date BETWEEN @PublishStart AND @PublishEnd
ORDER BY CAST(pm.meta_value AS INT) DESC;</CommandText>
      </Query>
      <Fields>
        <Field Name="post_title">
          <DataField>post_title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="guid">
          <DataField>guid</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="view_count">
          <DataField>view_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
