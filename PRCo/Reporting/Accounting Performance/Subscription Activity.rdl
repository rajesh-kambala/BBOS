  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>MAS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ComputedEndDate datetime
SET @ComputedEndDate = DATEADD(day, 1, @EndDate)

DECLARE @MyTable table (
	tmpCycleCode varchar(2)
)

DECLARE @ndx int = 1
WHILE (@ndx &lt;= 12) BEGIN
	INSERT INTO @MyTable SELECT RIGHT('0'+ CONVERT(VARCHAR, @ndx),2);
	SET @ndx = @ndx + 1
END


SELECT tmpCycleCode, 
      COUNT(DISTINCT prsoat_SalesOrderNo) As OrderCount,
       CASE 
			WHEN MONTH(@StartDate) &gt;= CAST(tmpCycleCode as Int) THEN DATEADD(Year, 1, CONVERT(datetime, tmpCycleCode + '/1/' + CAST(YEAR(@StartDate) As Varchar(4)),101))
			ELSE CONVERT(datetime, tmpCycleCode + '/1/' + CAST(YEAR(@StartDate) As Varchar(4)),101)
  		    END As CycleDate,
        ISNULL(SUM(prsoat_ExtensionAmtChange), 0) as TotalAmtChange
  FROM @MyTable
       LEFT OUTER JOIN CRM.dbo.PRSalesOrderAuditTrail WITH (NOLOCK) ON tmpCycleCode = prsoat_CycleCode AND prsoat_CreatedDate BETWEEN @StartDate AND @ComputedEndDate
GROUP BY tmpCycleCode
ORDER BY CycleDate</CommandText>
      </Query>
      <Fields>
        <Field Name="tmpCycleCode">
          <DataField>tmpCycleCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CycleDate">
          <DataField>CycleDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="OrderCount">
          <DataField>OrderCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalAmtChange">
          <DataField>TotalAmtChange</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
