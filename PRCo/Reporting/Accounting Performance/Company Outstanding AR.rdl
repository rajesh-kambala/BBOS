  <DataSets>
    <DataSet Name="dsInvoices">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT h.UDF_MASTER_INVOICE,
	   CAST(h.CustomerNo AS INT) As EnteredCompanyID,
       entered.comp_Name As EnteredName,
       enteredLocation.CityStateCountryShort as EnteredLocation,
	   SUM(o.Balance) As Balance
  FROM MAS_PRC.dbo.AR_OpenInvoice o
       INNER JOIN MAS_PRC.dbo.AR_InvoiceHistoryHeader h ON h.InvoiceNo = o.InvoiceNo
       INNER JOIN CRM.dbo.Company entered WITH (NOLOCK) ON CAST(h.CustomerNo AS INT) = entered.comp_CompanyID
       INNER JOIN CRM.dbo.vPRLocation enteredLocation WITH (NOLOCK) ON entered.comp_PRListingCityID = enteredLocation.prci_CityID
 WHERE CAST(o.CustomerNo AS INT) = @CompanyID
   AND o.Balance &gt; 0
GROUP BY h.UDF_MASTER_INVOICE,
	     CAST(h.CustomerNo AS INT),
         entered.comp_Name,
         enteredLocation.CityStateCountryShort
ORDER BY CAST(h.CustomerNo AS INT)</CommandText>
      </Query>
      <Fields>
        <Field Name="UDF_MASTER_INVOICE">
          <DataField>UDF_MASTER_INVOICE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EnteredName">
          <DataField>EnteredName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EnteredLocation">
          <DataField>EnteredLocation</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EnteredCompanyID">
          <DataField>EnteredCompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Balance">
          <DataField>Balance</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsBillTo">
      <Query>
        <DataSourceName>BBS</DataSourceName>
