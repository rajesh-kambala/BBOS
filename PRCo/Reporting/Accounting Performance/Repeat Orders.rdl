  </DataSources>
  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>MAS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT  [BBID] = comp_CompanyID
       ,[Business Name] = Company.comp_Name
	   ,[Listing City] = prci_City 
	   ,[Listing State] = ISNULL(prst_Abbreviation, prst_State)
	   ,[Listing Country] = prcn_Country
	   ,[Invoice Date] = InvoiceDate
	   ,[Anniversary Date] = ps.prse_NextAnniversaryDate
	   ,[Anniversary Month] = MONTH(ps.prse_NextAnniversaryDate)
	   ,[Service Level] = ps.prse_ServiceCode
	   ,[Total Order Amount] = AmtInvoiced
	   ,ro.SalesOrderNo
FROM CRM.dbo.vPRRepeatOrders ro
		INNER JOIN MAS_PRC.dbo.SO_SalesOrderHistoryHeader soh ON soh.SalesOrderNo = ro.SalesOrderNo
        INNER JOIN CRM.dbo.Company ON EnteredCompanyID = Comp_CompanyId 
        INNER JOIN CRM.dbo.vPRLocation ON comp_PRListingCityId = prci_CityID 
		INNER JOIN CRM.dbo.PRService ps WITH (NOLOCK) ON comp_CompanyID = ps.prse_CompanyID AND ps.prse_Primary = 'Y'
	WHERE 
		MONTH(ps.prse_NextAnniversaryDate)=CASE WHEN @Month=0 THEN MONTH(ps.prse_NextAnniversaryDate) ELSE @Month END
AND InvoiceDate IS NOT NULL
ORDER BY comp_Name</CommandText>
      </Query>
