  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>MAS</DataSourceName>
        <CommandText>SELECT
	CycleDate = CASE 
			WHEN MONTH(GETDATE()) &gt;= CAST(CycleCode AS INT) THEN DATEADD(Year, 1, CONVERT(DATETIME, CycleCode + '/1/' + CAST(Year(GETDATE()) AS VARCHAR(4)),101))
			ELSE CONVERT(DATETIME, CycleCode + '/1/' + CAST(Year(GETDATE()) AS VARCHAR(4)),101)
  		    END
	, Subtotal = SUM(TaxableAmt) + SUM(NonTaxableAmt)
    , Tax = SUM(SalesTaxAmt)
    , GrandTotal = SUM(SalesTaxAmt) + SUM(TaxableAmt) + SUM(NonTaxableAmt)
    , OrderCount = COUNT(SalesOrderNo)
    , InvoiceCount = COUNT(DISTINCT CASE WHEN BillToCustomerNo &lt;&gt; '' THEN BillToCustomerNo ELSE CustomerNo END)
FROM MAS_PRC.dbo.SO_SalesOrderHeader WITH (NOLOCK)
WHERE OrderType = 'R'  
  AND CycleCode &lt;&gt; '99'
GROUP BY CASE 
			WHEN MONTH(GETDATE()) &gt;= CAST(CycleCode AS INT) THEN DATEADD(Year, 1, CONVERT(DATETIME, CycleCode + '/1/' + CAST(Year(GETDATE()) AS VARCHAR(4)),101))
			ELSE CONVERT(DATETIME, CycleCode + '/1/' + CAST(Year(GETDATE()) AS VARCHAR(4)),101)
  		    END
ORDER BY CycleDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
