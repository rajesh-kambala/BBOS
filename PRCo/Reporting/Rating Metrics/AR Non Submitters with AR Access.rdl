  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DaysOld">
            <Value>=Parameters!DaysOld.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT comp_CompanyID [BBID],
       Company.comp_Name [Company Name],
	   CityStateCountryShort [Listing Location],
	   MostRecentARDate
  FROM Company WITH (NOLOCK)
       INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
	   LEFT OUTER JOIN (SELECT praa_CompanyID, MAX(praa_CreatedDate) as MostRecentARDate
					  FROM Company WITH (NOLOCK)
						   INNER JOIN PRARAging WITH (NOLOCK) ON comp_CompanyID = praa_CompanyID
					 WHERE comp_PRIndustryType IN ('P', 'T', 'S')
					 GROUP BY praa_CompanyID) T1 ON comp_CompanyID = praa_CompanyID
LEFT OUTER JOIN  PRCompanyInfoProfile prcip WITH (NOLOCK) ON prcip.prc5_CompanyId = comp_companyid

 WHERE prc5_PRARReportAccess = 'Y' 
   AND (MostRecentARDate IS NULL
        OR MostRecentARDate &lt; DATEADD(day, 0-@DaysOld, GETDATE()))
 ORDER BY comp_Name;</CommandText>
      </Query>
      <Fields>
        <Field Name="BBID">
          <DataField>BBID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Company_Name">
          <DataField>Company Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Listing_Location">
          <DataField>Listing Location</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MostRecentARDate">
          <DataField>MostRecentARDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
