  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ThresholdDate">
            <Value>=Parameters!ThresholdDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT comp_CompanyID [BBID],
       comp_Name [Company Name],
	   CityStateCountryShort as [Listing Location],
	   comp_PRIndustryType as [Industry],
	   MAX(praa_CreatedDate) as [Last AR Submission Date]
  FROM Company
       INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
       INNER JOIN PRARAging ON comp_CompanyID = praa_CompanyId
			 LEFT OUTER JOIN PRCompanyInfoProfile WITH (NOLOCK) ON comp_CompanyID = prc5_CompanyID
 WHERE prc5_PRARReportAccess = 'Y'
   AND comp_PRIndustryType IN ('P', 'T', 'S')
GROUP BY comp_CompanyID, comp_Name, comp_PRIndustryType, CityStateCountryShort
HAVING MAX(praa_CreatedDate) &lt; @ThresholdDate
ORDER BY MAX(praa_CreatedDate), comp_Name</CommandText>
      </Query>
      <Fields>
        <Field Name="BBID">
          <DataField>BBID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Company_Name">
          <DataField>Company Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Listing_Location">
          <DataField>Listing Location</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Industry">
          <DataField>Industry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Last_AR_Submission_Date">
          <DataField>Last AR Submission Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
