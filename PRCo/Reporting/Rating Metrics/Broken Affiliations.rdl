  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>DECLARE @Affilations table (
    ndx int identity(1, 1),
	LeftCompanyID int,
	RightCompanyID int,
	Broken bit)


INSERT INTO @Affilations (LeftCompanyID, RightCompanyID)
SELECT l.comp_CompanyID,
	   r.comp_CompanyID
  FROM PRCompanyRelationship
       INNER JOIN Company l ON l.comp_CompanyID = prcr_LeftCompanyID
	   INNER JOIN Company r ON r.comp_CompanyID = prcr_RightCompanyID
 WHERE prcr_Type = '29'
   AND prcr_Active = 'Y'
   AND l.comp_PRListingStatus IN ('L', 'H')
   AND r.comp_PRListingStatus IN ('L', 'H')

DECLARE @Count int, @Index int
DECLARE @LeftCompanyID int, @RightCompanyID int, @CommonCount int

SELECT @Count = COUNT(1)
  FROM @Affilations

SET @Index = 0

WHILE (@Index &lt; @Count) BEGIN
	SET @Index = @Index + 1

	SELECT @LeftCompanyID = LeftCompanyID,
	       @RightCompanyID = RightCompanyID
	  FROM @Affilations
	 WHERE ndx = @Index

	SET @CommonCount = 0
	SELECT @CommonCount = COUNT(1)
	  FROM 
			(SELECT peli_PersonID, peli_CompanyID, peli_PRStatus, peli_PROwnershipRole
			  FROM Person_Link
			 WHERE peli_PRStatus IN ('1', '2')
			   AND peli_PROwnershipRole IN ('RCO', 'RCN', 'RCU')
			   AND peli_CompanyID = @LeftCompanyID) T1
			INNER JOIN (
				SELECT peli_PersonID, peli_CompanyID, peli_PRStatus, peli_PROwnershipRole
				  FROM Person_Link
				 WHERE peli_PRStatus IN ('1', '2')
				   AND peli_PROwnershipRole IN ('RCO', 'RCN', 'RCU')
				   AND peli_CompanyID = @RightCompanyID) T2 ON T1.peli_PersonID = T2.peli_PersonID
	  
	IF @CommonCount = 0 BEGIN

		UPDATE @Affilations
		   SET Broken = 1
		WHERE LeftCompanyID = @LeftCompanyID
		  AND RightCompanyID = @RightCompanyID
	END
END

SELECT l.comp_CompanyID as [Company1 BBID],
       l.comp_Name as [Company1 Name],
       ll.CityStateCountryShort as [Company1 Location],
	   r.comp_CompanyID as [Company2 BBID],
       r.comp_Name as [Company2 Name],
       rl.CityStateCountryShort as [Company2 Location]
 FROM @Affilations
      INNER JOIN Company l ON l.comp_CompanyID = LeftCompanyID
	  INNER JOIN vPRLocation ll ON l.comp_PRListingCityID = ll.prci_CityID
	  INNER JOIN Company r ON r.comp_CompanyID = RightCompanyID
	  INNER JOIN vPRLocation rl ON l.comp_PRListingCityID = rl.prci_CityID
 WHERE Broken = 1
ORDER BY l.comp_name, r.comp_Name</CommandText>
