  <DataSets>
    <DataSet Name="dsSummary">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SubmitterCount">
            <Value>=Parameters!SubmitterCount.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT COUNT(DISTINCT praa_CompanyID) as DistinctSubmitterCount,
       COUNT(DISTINCT praad_ARAgingDetailID) as ARFileCount,
	   MAX(praad_FileCompanyName) [CompanyName],
       dbo.ufn_GetLowerAlpha(dbo.ufn_PrepareCompanyName(praad_FileCompanyName)) CompanyNameMatch
 FROM Company WITH (NOLOCK)
      INNER JOIN PRARAging WITH (NOLOCK) ON comp_CompanyID = praa_CompanyID
      INNER JOIN PRARAgingDetail WITH (NOLOCK) ON praa_ARAgingID = praad_ARAgingID
WHERE comp_PRIndustryType IN ('P', 'T')
  AND (praad_SubjectCompanyID IS NULL OR praad_SubjectCompanyID = 0)
  AND praad_FileCompanyName IS NOT NULL
  AND praad_FileCompanyName &lt;&gt; ''
  AND praad_FileCompanyName NOT IN ('Cash', 'Cash Sales')
  --AND ISNULL(praad_FileCityName, '') = CASE @HasCityState WHEN 1 THEN ISNULL(praad_FileCityName, '-z-') ELSE ISNULL(praad_FileCityName, '') END
GROUP BY dbo.ufn_GetLowerAlpha(dbo.ufn_PrepareCompanyName(praad_FileCompanyName)),
	   praad_FileCityName,
	   praad_FileStateName
HAVING COUNT(DISTINCT praa_CompanyID) &gt;= @SubmitterCount
ORDER BY COUNT(DISTINCT praa_CompanyID) DESC, COUNT(DISTINCT praad_ARAgingDetailID) DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="DistinctSubmitterCount">
          <DataField>DistinctSubmitterCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ARFileCount">
          <DataField>ARFileCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CompanyName">
          <DataField>CompanyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyNameMatch">
          <DataField>CompanyNameMatch</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
