  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT RTRIM(ISNULL(user_logon, 'BBOS')) as [User],
	   COUNT(DISTINCT prcl2_ConnectionListID) as [Count],
	   --COUNT(prclc_RelatedCompanyID) as BBIDCount,
	   CAST(COUNT(prclc_RelatedCompanyID) as Decimal(10,3)) / CAST(COUNT(DISTINCT prcl2_ConnectionListID) as Decimal(10,3)) as [Ave CL Size]
  FROM PRConnectionList WITH (NOLOCK)
       LEFT OUTER JOIN users ON prcl2_CreatedBy = user_userid
       LEFT OUTER JOIN PRConnectionListCompany WITH (NOLOCK) ON prcl2_ConnectionListID = prclc_ConnectionListID
WHERE prcl2_CreatedDate BETWEEN @StartDate AND  DATEADD(day, 1, @EndDate)
GROUP BY RTRIM(ISNULL(user_logon, 'BBOS'))
ORDER BY RTRIM(ISNULL(user_logon, 'BBOS'))</CommandText>
      </Query>
      <Fields>
        <Field Name="User">
          <DataField>User</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Count">
          <DataField>Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Ave_CL_Size">
          <DataField>Ave CL Size</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="StartDate">
      <DataType>DateTime</DataType>
      <DefaultValue>
        <Values>
          <Value>=DateAdd("M", -1,DateValue(CStr(Month(Now)) &amp; "/1/" &amp; CStr(Year(Now))))</Value>
        </Values>
      </DefaultValue>
      <Prompt>Start Date</Prompt>
    </ReportParameter>
    <ReportParameter Name="EndDate">
      <DataType>DateTime</DataType>
      <DefaultValue>
        <Values>
          <Value>=DateAdd("d", -1, DateValue(CStr(Month(Now)) &amp; "/1/" &amp; CStr(Year(Now))))</Value>
        </Values>
      </DefaultValue>
      <Prompt>End Date</Prompt>
    </ReportParameter>
  </ReportParameters>
