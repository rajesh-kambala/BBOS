  <DataSets>
    <DataSet Name="BBS">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Threshold">
            <Value>=Parameters!Threshold.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AnalystId">
            <Value>=Parameters!AnalystId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @d0 datetime, @d1 datetime, @d2 datetime

select @d0 = max(prbs_Date)
from PRBBScore

select @d1 = max(prbs_Date)
from PRBBScore
where prbs_Date &lt; @d0

select @d2 = max(prbs_Date)
from PRBBScore
where prbs_Date &lt; @d1

select t0.prbs_BBScoreId, dbo.ufn_GetPRCoSpecialistUserId(comp_CompanyId,0) as AnalystId,
 dbo.ufn_FormatUserName(dbo.ufn_GetPRCoSpecialistUserId(comp_CompanyId,0)) as Analyst,
 t0.prbs_CompanyId as BBID, 
 comp_PRBookTradestyle as TradeName,
 CityStateCountryShort as Location,
 prra_RatingLine as CurrentRating,
 cast(abs(t0.prbs_ConfidenceScore - t1.prbs_ConfidenceScore) as integer) as ConfChange,
 cast(t0.prbs_ConfidenceScore as integer) as ConfLevel,
 cast(t1.prbs_ConfidenceScore as integer) as ConfOneMonthAgo,
 cast(t2.prbs_ConfidenceScore as integer) as ConfTwoMonthsAgo,
 cast(t0.prbs_BBScore as integer) as CurrentScore,
 cast(t1.prbs_BBScore as integer) as ScoreOneMonthAgo,
 cast(t2.prbs_BBScore as integer) as ScoreTwoMonthsAgo
from PRBBScore t0
join Company on comp_CompanyId = prbs_CompanyId
join vPRLocation on prci_CityId = comp_PRListingCityId
left outer join vPRCompanyRating on prra_CompanyId = comp_CompanyId and prra_Current = 'Y'
inner join PRBBScore t1 
 on t1.prbs_CompanyId = t0.prbs_CompanyId and t1.prbs_Date = @d1
left outer join PRBBScore t2
 on t2.prbs_CompanyId = t0.prbs_CompanyId and t2.prbs_Date = @d2
where t0.prbs_Date = @d0
and t0.prbs_ConfidenceScore &gt; 0
and t0.prbs_ConfidenceScore &lt; @Threshold
and t1.prbs_ConfidenceScore &gt;= @Threshold
and comp_PRInvestigationMethodGroup = 'A'
and comp_PRType = 'H'
and dbo.ufn_GetPRCoSpecialistUserId(comp_CompanyId,0) = 
 case when @AnalystId = -1
 then dbo.ufn_GetPRCoSpecialistUserId(comp_CompanyId,0) 
 else @AnalystId 
end 
order by abs(t0.prbs_ConfidenceScore - t1.prbs_ConfidenceScore) desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
