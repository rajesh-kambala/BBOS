          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCompanyConnectionLists">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT null as prcl2_ConnectionListID, 'Global Connection List' as prcl2_ConnectionListDate
 UNION
SELECT prcl2_ConnectionListID, CAST(CAST(prcl2_ConnectionListDate as Date) as varchar(25)) as prcl2_ConnectionListDate
  FROM PRConnectionList WITH (NOLOCK)
WHERE prcl2_CompanyID = @CompanyID
ORDER BY prcl2_ConnectionListDate DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="prcl2_ConnectionListID">
          <DataField>prcl2_ConnectionListID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="prcl2_ConnectionListDate">
          <DataField>prcl2_ConnectionListDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsConnectionList">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ConnectionListID">
            <Value>=Parameters!ConnectionListID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BlankRowCount">
            <Value>=Parameters!BlankRowCount.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @MyTable table (
	CompanyID int,
	CompanyName varchar(200),
	CityStateContryShort varchar(200),
	prcr_Type varchar(40),
	CompanyPhone varchar(50)
)

IF (@ConnectionListID IS NOT NULL) BEGIN

	INSERT INTO @MyTable
	SELECT CompanyID, CompanyName, CityStateCountryShort, prcr_Type, dbo.ufn_FormatPhone(phon_CountryCode, phon_AreaCode, phon_Number, null) as CompanyPhone
	  FROM vPRConnectionListDetailsType
	       LEFT OUTER JOIN vPRCompanyPhone WITH (NOLOCK) ON plink_RecordID = CompanyID AND phon_PRIsPhone = 'Y' AND phon_PRPreferredPublished = 'Y'
	 WHERE prcl2_ConnectionListID = @ConnectionListID
     ORDER BY CompanyName

END ELSE BEGIN

	INSERT INTO @MyTable
	SELECT comp_CompanyID As CompanyID, comp_Name as CompanyName, CityStateCountryShort, prcr_Type, dbo.ufn_FormatPhone(phon_CountryCode, phon_AreaCode, phon_Number, null) as CompanyPhone
	  FROM PRCompanyRelationship WITH (NOLOCK)
	       INNER JOIN Company WITH (NOLOCK) ON prcr_RightCompanyID = comp_CompanyID
		   INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
		   LEFT OUTER JOIN vPRCompanyPhone WITH (NOLOCK) ON plink_RecordID = comp_CompanyID AND phon_PRIsPhone = 'Y' AND phon_PRPreferredPublished = 'Y'
	 WHERE prcr_Type IN ('09','10','11','12','13','14','15','16')
	  AND prcr_LeftCompanyId = @CompanyID
	  AND prcr_Active = 'Y'
ORDER BY comp_Name 

END

DECLARE @Count int = 0

WHILE (@Count &lt; @BlankRowCount) BEGIN

	INSERT INTO @MyTable VALUES (null, null, null, 'blank', null)
	SET @Count = @Count + 1
END

SELECT * FROM @MyTable</CommandText>
      </Query>
      <Fields>
        <Field Name="CompanyID">
          <DataField>CompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CompanyName">
          <DataField>CompanyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CityStateContryShort">
          <DataField>CityStateContryShort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="prcr_Type">
          <DataField>prcr_Type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyPhone">
          <DataField>CompanyPhone</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCounts">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT SUM(CASE prcr_Type WHEN '09' THEN 1 ELSE 0 END) as VendorCount,
       SUM(CASE prcr_Type WHEN '13' THEN 1 ELSE 0 END) as CustomerCount,
	   SUM(CASE WHEN prcr_Type NOT IN ('09', '13') THEN 1 ELSE 0 END) as OtherCount
  FROM Company WITH (NOLOCK)
       INNER JOIN PRCompanyRelationship WITH (NOLOCK) ON comp_CompanyID = prcr_LeftCompanyID
 WHERE comp_CompanyID = @CompanyID
   AND prcr_Active = 'Y'
   AND prcr_Type IN ('09', '10', '11', '12', '13', '15')</CommandText>
      </Query>
      <Fields>
        <Field Name="VendorCount">
          <DataField>VendorCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CustomerCount">
          <DataField>CustomerCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="OtherCount">
          <DataField>OtherCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
