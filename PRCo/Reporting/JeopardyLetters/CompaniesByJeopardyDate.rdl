    <DataSet Name="dsCommunicationLanguage">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT 'All' capt_code, 'All' capt_us
UNION ALL
SELECT RTRIM(capt_code) AS capt_code, capt_us
  FROM custom_captions 
 WHERE capt_family = 'comp_PRCommunicationLanguage'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="capt_code">
          <DataField>capt_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="capt_us">
