  <DataSets>
    <DataSet Name="dsCompanies">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TMFM">
            <Value>=Parameters!TMFM.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CommunicationLanguage">
            <Value>=Parameters!CommunicationLanguage.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndustryType">
            <Value>=Parameters!IndustryType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @ReportTable table (
	CompanyID int,
	CompanyName varchar(104),
               JeopardyDate datetime,
                StatementDate datetime,
	StatementType varchar(40),
                RatingLine varchar(150),
                Location varchar(500),
               comp_PRTMFMAward char(1)
)


INSERT INTO @ReportTable (CompanyID, CompanyName, JeopardyDate, RatingLine, Location, comp_PRTMFMAward )
SELECT comp_CompanyID, Company.comp_Name, comp_PRJeopardyDate, prra_RatingLine, CityStateCountryShort, comp_PRTMFMAward 
  FROM COMPANY
             INNER JOIN vPRCompanyRating ON comp_CompanyID = prra_CompanyID AND prra_Current = 'Y' and (prra_CreditWorthId is not null and prra_CreditWorthId not in (4,7,8,9,10))
             INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
 WHERE comp_PRJeopardyDate BETWEEN @StartDate AND @EndDate
      AND comp_PRType = 'H'                   -- HQ Company
      AND comp_PRListingStatus NOT IN ('D', 'N3', 'N5', 'N6')  -- Listed or On Hold
      AND ISNULL(comp_PRTMFMAward, 'N') = CASE @TMFM WHEN 'Y' THEN 'Y' WHEN 'N' THEN 'N'  ELSE ISNULL(comp_PRTMFMAward, 'N') END
      AND comp_PRCommunicationLanguage = CASE @CommunicationLanguage WHEN 'All' THEN comp_PRCommunicationLanguage ELSE @CommunicationLanguage  END
      AND prra_CreditWorthID IS NOT NULL      -- Must have a Credit Worth Rating
AND comp_PRIndustryType IN (@IndustryType);

UPDATE @ReportTable
   SET StatementDate = prfs_StatementDate
  FROM (SELECT prfs_CompanyID, MAX(prfs_StatementDate) AS prfs_StatementDate
          FROM PRFinancial
               INNER JOIN @ReportTable on prfs_CompanyID = CompanyID
        GROUP BY prfs_CompanyID) T1
 WHERE CompanyID = prfs_CompanyID;

-- This has to be done in a separate step because if a company has both year-end and
-- interim statements, the group by in the previous statement would result in multiple
-- rows
UPDATE @ReportTable
   SET StatementType = prfs_Type
  FROM (SELECT prfs_CompanyID, prfs_Type
          FROM PRFinancial
               INNER JOIN @ReportTable on prfs_CompanyID = CompanyID AND prfs_StatementDate = StatementDate) T1
 WHERE CompanyID = prfs_CompanyID;


SELECT CompanyID, CompanyName, JeopardyDate, StatementDate, c.capt_us AS StatementType, RatingLine, Location, comp_PRTMFMAward, dbo.ufn_IsActiveMember(CompanyID) as IsMember
  FROM @ReportTable
       INNER JOIN Custom_Captions c ON StatementType = c.capt_code AND c.capt_family = 'prfs_Type'
ORDER BY StatementDate, CompanyName;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CompanyID">
          <DataField>CompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CompanyName">
          <DataField>CompanyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
