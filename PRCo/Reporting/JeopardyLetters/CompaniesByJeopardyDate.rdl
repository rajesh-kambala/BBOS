        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT RTRIM(capt_code) as capt_code, capt_us
  FROM Custom_Captions
 WHERE capt_family = 'comp_PRIndustryType'
ORDER BY capt_order</CommandText>
