  <DataSets>
    <DataSet Name="dsCompanies">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @HQID int = dbo.ufn_BRGetHQID(@CompanyID)

SELECT DISTINCT comp_CompanyID, comp_Name, CityStateCountryShort,
       CASE 
			WHEN comp_CompanyID = @HQID  THEN 'HQ'
			WHEN comp_PRHQID = @HQID THEN 'Branches'
			ELSE 'Affiliates' END As RelationshipType,
       CASE 
			WHEN comp_CompanyID = @HQID  THEN 1
			WHEN comp_PRHQID = @HQID THEN 2
			ELSE 3 END As SortOrder,
		COUNT(prse_ServiceCode) As ServiceCount			
  FROM Company WITH (NOLOCK)
       INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
       LEFT OUTER JOIN PRCompanyRelationship lft WITH (NOLOCK) ON  lft.prcr_LeftCompanyID = comp_CompanyID AND lft.prcr_Type IN ('27', '28', '29') AND lft.prcr_Active = 'Y'
       LEFT OUTER JOIN PRCompanyRelationship rght WITH (NOLOCK) ON rght.prcr_RightCompanyID = comp_CompanyID AND rght.prcr_Type IN ('27', '28', '29') AND rght.prcr_Active = 'Y'
       LEFT OUTER JOIN PRService WITH (NOLOCK) ON Comp_CompanyId = prse_CompanyID
 WHERE (comp_PRHQID = @HQID
    OR ISNULL(lft.prcr_RightCompanyId, 0) =@HQID
    OR ISNULL(rght.prcr_RightCompanyId, 0) =@HQID)
AND comp_PRListingStatus NOT IN ('N3', 'N5', 'N6', 'D')
GROUP BY comp_CompanyID, comp_Name, CityStateCountryShort, comp_PRHQID
ORDER BY SortOrder</CommandText>
      </Query>
      <Fields>
        <Field Name="comp_CompanyID">
          <DataField>comp_CompanyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="comp_Name">
          <DataField>comp_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CityStateCountryShort">
          <DataField>CityStateCountryShort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RelationshipType">
          <DataField>RelationshipType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SortOrder">
          <DataField>SortOrder</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ServiceCount">
          <DataField>ServiceCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCompany">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prcse_FullName
  FROM CRM.dbo.PRCompanySearch WITH (NOLOCK)
 WHERE prcse_CompanyID = @CompanyID  </CommandText>
      </Query>
      <Fields>
        <Field Name="prcse_FullName">
