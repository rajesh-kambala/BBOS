  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Ind">
            <Value>=Parameters!Ind.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--# Listed HQ between two dates

DECLARE @Companies TABLE (BBID INT PRIMARY KEY, Contacted INT)

DECLARE @Interactions TABLE (BBID INT)

--# Listed HQ between two dates

		INSERT INTO @Companies (BBID)
		SELECT comp_CompanyID
		FROM Company WITH (NOLOCK)
		WHERE comp_PRType = 'H'
		AND comp_PRIndustryType IN (@Ind)
		AND (
			(comp_PRListingStatus IN ('L','H') 
				AND (comp_PRListedDate IS NULL OR comp_PRListedDate &lt;= @EndDate))
			OR comp_PRListingStatus NOT IN ('L', 'H') AND comp_PRDelistedDate &gt; @StartDate);



		INSERT INTO @Interactions
			SELECT DISTINCT CmLi_Comm_CompanyId 
			FROM CRMArchive.dbo.Comm_Link WITH (NOLOCK)
			INNER JOIN CRMArchive.dbo.Communication WITH (NOLOCK) ON Comm_CommunicationId = CmLi_Comm_CommunicationId
			WHERE CmLi_CreatedDate BETWEEN @StartDate AND @EndDate
			AND CmLi_CreatedBy &gt; 1
			AND Comm_Action IN ('EmailIn', 'EmailOut', 'FaxIn', 'FaxOut', 'LetterIn', 'LetterOut', 'PhoneIn','PhoneOut')
			AND CmLi_Comm_CompanyId IS NOT NULL;


		INSERT INTO @Interactions
			SELECT DISTINCT CmLi_Comm_CompanyId 
			FROM Comm_Link WITH (NOLOCK)
			INNER JOIN Communication WITH (NOLOCK) ON Comm_CommunicationId = CmLi_Comm_CommunicationId
			WHERE CmLi_CreatedDate BETWEEN @StartDate AND @EndDate
			AND CmLi_CreatedBy &gt; 1
			AND Comm_Action IN ('EmailIn', 'EmailOut', 'FaxIn', 'FaxOut', 'LetterIn', 'LetterOut', 'PhoneIn','PhoneOut')
			AND CmLi_Comm_CompanyId IS NOT NULL
			AND CmLi_Comm_CompanyId NOT IN (SELECT BBID FROM @Companies);

		INSERT INTO @Interactions
			SELECT DISTINCT prtx_CompanyId 
			FROM PRTransaction WITH (NOLOCK)
			WHERE prtx_CreatedDate BETWEEN @StartDate AND @EndDate
			AND prtx_CreatedBy &gt; 1
			AND prtx_CompanyId NOT IN (SELECT BBID FROM @Companies);
			
		INSERT INTO @Interactions
			SELECT DISTINCT prtx_CompanyId 
			FROM crmarchive.dbo.PRTransaction WITH (NOLOCK)
			WHERE prtx_CreatedDate BETWEEN @StartDate AND @EndDate
			AND prtx_CreatedBy &gt; 1
			AND prtx_CompanyId NOT IN (SELECT BBID FROM @Companies);
			

		UPDATE @Companies
		SET Contacted = 1
		FROM @Companies c
		INNER JOIN @Interactions i ON i.BBID = c.BBID
		WHERE Contacted IS NULL


SELECT ListedCompanies = COUNT(BBID)
	, NumContacted = SUM(contacted)
FROM @Companies</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
