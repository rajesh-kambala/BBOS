        <CommandText>DECLARE @C Table (
	ndx int identity,
	BBID int,
	CompanyName varchar(1000),
	City varchar(1000),
	[State] varchar(1000),
	Country varchar(1000),
	ServiceCode varchar(1000),
	ServiceStartDate datetime,
	AnniversaryMonth int,
	Quantity decimal(15,6),
	Discount decimal(8,3),
	DateLastInvoiced datetime,
	AmountLastInvoiced decimal(12,2),
	LastInvoiceNum varchar(7),
	DatePaid datetime,
	AmountPaid decimal(12,2),
	AmountOwing decimal(12,2)
)

--Initial Company Insert
INSERT INTO @C(BBID, CompanyName, City, [State], Country, ServiceCode, ServiceStartDate, AnniversaryMonth, Quantity, Discount, AmountLastInvoiced)
SELECT DISTINCT
	comp_CompanyID,
	comp_Name,
	prci_City,
	prst_State,
	prcn_Country,
	prse_ServiceCode,
	comp_PROriginalServiceStartDate,
	MONTH(prse_NextAnniversaryDate),
	QuantityOrdered,
	prse_DiscountPct,
	ExtensionAmt
FROM Company WITH (NOLOCK)
	INNER JOIN vPRLocation WITH (NOLOCK) on prci_CityID = comp_PRListingCityID
	INNER JOIN PRService WITH(NOLOCK) ON comp_CompanyID = prse_CompanyID AND prse_primary = 'Y'
WHERE
	comp_PRIndustryType = 'L' --Lumber
	AND ISNULL(prse_ServiceCode,'') IN ('L100', 'L150', 'L201', 'L301')
ORDER BY Comp_Name

DECLARE @ndx int; Set @ndx = 0
DECLARE @CompanyCount int = (SELECT COUNT(*) FROM @C)
DECLARE @BBID int, @ServiceCode varchar(1000), @MasterInvoiceNum varchar(500), @MasterInvoiceDate datetime
DECLARE @DatePaid datetime, @AmountPaid decimal(12,2)
DECLARE @AmountOwing decimal(12,2)

WHILE (@ndx &lt; @CompanyCount) BEGIN 
	SET @ndx = @ndx + 1
	SET @MasterInvoiceNum = ''
	SET @MasterInvoiceDate = NULL
	SET @DatePaid = NULL
	SET @AmountPaid = NULL
	SET @AmountOwing = NULL;
	
	SELECT @BBID=BBID FROM @C WHERE ndx=@ndx
	SELECT @ServiceCode=ServiceCode FROM @C WHERE ndx=@ndx

	SELECT TOP 1 @MasterInvoiceNum=id.UDF_MASTER_INVOICE
	  FROM MAS_PRC.dbo.vBBSIMasterInvoiceDetails id with(nolock) 
	 WHERE BillToCustomerNo=@BBID AND ItemCode=@ServiceCode ORDER BY JournalNoGLBatchNo DESC
	
	SELECT TOP 1 @MasterInvoiceDate=InvoiceDate FROM MAS_PRC.dbo.vBBSIMasterInvoices i with(nolock) WHERE UDF_MASTER_INVOICE=@MasterInvoiceNum ORDER BY JournalNoGLBatchNo DESC
	
	SELECT TOP 1 @DatePaid=TransactionDate, @AmountPaid=ABS(TransactionAmt) FROM dbo.ufn_GetPaymentHistory(@BBID, NULL) 
	WHERE TransactionType='P' AND UDF_MASTER_INVOICE=@MasterInvoiceNum
	ORDER BY Sequence

	SELECT @AmountOwing = SUM(o.Balance) FROM MAS_PRC.dbo.AR_OpenInvoice o INNER JOIN MAS_PRC.dbo.AR_InvoiceHistoryHeader h ON h.InvoiceNo = o.InvoiceNo AND h.HeaderSeqNo = o.InvoiceHistoryHeaderSeqNo WHERE o.Balance &gt; 0 AND CAST(o.CustomerNo AS INT) = @BBID

	UPDATE @C SET DateLastInvoiced=@MasterInvoiceDate, LastInvoiceNum=@MasterInvoiceNum,
		          DatePaid=@DatePaid, AmountPaid=@AmountPaid, AmountOwing=@AmountOwing
	 WHERE ndx=@ndx
END

SELECT * FROM @C ORDER BY CompanyName</CommandText>
