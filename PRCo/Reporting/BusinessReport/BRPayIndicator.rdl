  <DataSets>
    <DataSet Name="dsPayIndicator">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prcpi_PayIndicator,
             dbo.ufn_GetCustomCaptionValue('PayIndicatorDescription', prcpi_PayIndicator, 'en-us') As Description
 FROM PRCompanyPayIndicator WITH (NOLOCK)
WHERE prcpi_CompanyID =dbo.ufn_BRGetHQID(@CompanyID)
     AND prcpi_Current = 'Y'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="prcpi_PayIndicator">
          <DataField>prcpi_PayIndicator</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsLastChanged">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @PayIndicator varchar(40)
DECLARE @TempID int

DECLARE @HQID int
SET @HQID = dbo.ufn_BRGetHQID(@CompanyID)

DECLARE @DateLastChanged datetime
DECLARE @PreviousPayIndicator varchar(10)

SELECT @PayIndicator = prcpi_PayIndicator
  FROM PRCompanyPayIndicator WITH (NOLOCK)
 WHERE prcpi_CompanyID = @HQID
   AND prcpi_Current = 'Y'

SELECT @TempID = MAX(prcpi_CompanyPayIndicatorID)
  FROM PRCompanyPayIndicator WITH (NOLOCK)
 WHERE prcpi_CompanyID = @HQID
   AND ISNULL(prcpi_PayIndicator, 'x') &lt;&gt; ISNULL(@PayIndicator, 'x');

SELECT @DateLastChanged = MIN(prcpi_CreatedDate)
  FROM PRCompanyPayIndicator WITH (NOLOCK)
 WHERE prcpi_CompanyID = @HQID
   AND prcpi_CompanyPayIndicatorID &gt; @TempID
   
IF @DateLastChanged IS NULL BEGIN   
	SELECT @DateLastChanged = MIN(prcpi_CreatedDate)
	  FROM PRCompanyPayIndicator WITH (NOLOCK)
	 WHERE prcpi_CompanyID = @HQID;
END
   
   
SELECT @PreviousPayIndicator = prcpi_PayIndicator
  FROM PRCompanyPayIndicator WITH (NOLOCK)
 WHERE prcpi_CompanyPayIndicatorID = @TempID

SELECT @DateLastChanged As DateLastChanged, 
            @PreviousPayIndicator as PreviousPayIndicator,
            dbo.ufn_GetCustomCaptionValue('PayIndicatorDescription', @PreviousPayIndicator, 'en-us') As PreviousPayIndicatorDescription
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DateLastChanged">
          <DataField>DateLastChanged</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PreviousPayIndicator">
          <DataField>PreviousPayIndicator</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PreviousPayIndicatorDescription">
          <DataField>PreviousPayIndicatorDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
