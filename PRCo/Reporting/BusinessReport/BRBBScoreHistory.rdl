    <DataSet Name="dsCurrentRunDate">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IndustryType">
            <Value>=Parameters!IndustryType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT MAX(prbs_RunDate) AS CurrentRunDate
 FROM PRBBScore WITH (NOLOCK)
      INNER JOIN Company  WITH (NOLOCK) ON prbs_CompanyID = comp_CompanyID
WHERE comp_PRIndustryType = CASE @IndustryType WHEN 'L' THEN 'L' ELSE 'P' END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CurrentRunDate">
          <DataField>CurrentRunDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsBBScore_Exists">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@HistoryThresholdMonths">
            <Value>=Parameters!HistoryThreshold.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CurrentRunDate">
            <Value>=Parameters!CurrentRunDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select
    Case When Exists(
                Select *
                From   PRBBScore WITH (NOLOCK)
                LEFT OUTER JOIN Company WITH(NOLOCK) ON comp_CompanyID = prbs_CompanyID
                Where  prbs_CompanyID = @CompanyID
                       And prbs_PRPublish = 'Y'
                       AND comp_PRPublishBBScore='Y' 
                       And prbs_RunDate &gt;= DateAdd(month, -1 * @HistoryThresholdMonths, DateAdd(month, DateDiff(month, 0, @CurrentRunDate), 0))
              ) Then 1
         Else 0
    End As "BBScore_Exists"</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="BBScore_Exists">
          <DataField>BBScore_Exists</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
