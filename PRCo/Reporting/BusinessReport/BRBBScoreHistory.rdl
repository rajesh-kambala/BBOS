  <DataSets>
    <DataSet Name="dsBBScore_Current">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CurrentRunDate">
            <Value>=Parameters!CurrentRunDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select prbs_BBScore, prbs_Model, comp_PRIndustryType, prbs_RunDate, prbs_IndustryAve, prbs_IndustryPercentile from PRBBScore WITH (NOLOCK)
            LEFT OUTER JOIN Company WITH(NOLOCK) ON comp_CompanyID = prbs_CompanyID
            where prbs_CompanyId = @CompanyId 
                and prbs_PRPublish = 'Y'
                AND comp_PRPublishBBScore='Y' 
                and prbs_RunDate = @CurrentRunDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="prbs_BBScore">
          <DataField>prbs_BBScore</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
