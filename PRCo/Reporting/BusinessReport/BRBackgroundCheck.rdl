  <DataSets>
    <DataSet Name="Results">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prbcr2_BackgroundCheckID, 
	prbcr2_ResponseCode, 
	cc2.Capt_US [Response],
	prbcr2_QuestionCode, 
	cc3.Capt_US [Question], 
	prbcr2_SubjectCode, 
	cc1.Capt_US [Subject], 
	vPRBackgroundCheckResponse.Capt_Order
FROM vPRBackgroundCheck
	INNER JOIN vPRBackgroundCheckResponse ON prbc_BackgroundCheckID = prbcr2_BackgroundCheckID
	LEFT OUTER JOIN Custom_Captions cc1 ON cc1.Capt_Family='prbcr2_SubjectCode' and cc1.capt_code = prbcr2_SubjectCode
	LEFT OUTER JOIN Custom_Captions cc2 ON cc2.Capt_Family='prbcr2_ResponseCode' and cc2.capt_code = prbcr2_ResponseCode
	LEFT OUTER JOIN Custom_Captions cc3 ON cc3.Capt_Family='prbcr2_QuestionCode' and cc3.capt_code = prbcr2_QuestionCode
WHERE
	prbc_SubjectCompanyID = @CompanyID
ORDER BY
	prbcr2_SubjectCode, vPRBackgroundCheckResponse.Capt_Order</CommandText>
      </Query>
      <Fields>
        <Field Name="prbcr2_BackgroundCheckID">
