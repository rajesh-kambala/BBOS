          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="prdr_LicenseNumber">
          <DataField>prdr_LicenseNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
