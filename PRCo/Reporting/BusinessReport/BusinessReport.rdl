    <DataSet Name="dsIncludeEquifaxData">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IncludeEquifaxData">
            <Value>=Parameters!IncludeEquifaxData.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportLevel">
            <Value>=Parameters!ReportLevel.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Declare @Enabled int
SET @Enabled = 0

IF (dbo.ufn_GetCustomCaptionValue('EquifaxIntegration', 'Enabled', 'en-us') = '1') BEGIN
     IF (@IncludeEquifaxData = 1) BEGIN
            IF (@ReportLevel &gt;= 4) BEGIN

				DECLARE @ExcludeAsEquifaxSubject varchar(1)
				DECLARE @IndustryType varchar(1)
				DECLARE @ListingStatus varchar(40)

				SELECT	@ExcludeAsEquifaxSubject = ISNULL(comp_PRExcludeAsEquifaxSubject, 'N'),
						@ListingStatus = comp_PRListingStatus,
						@IndustryType = comp_PRIndustryType
				FROM Company WITH (NOLOCK) 
				WHERE comp_CompanyID = dbo.ufn_BRGetHQID(@CompanyID);

				IF 
				(
					@ExcludeAsEquifaxSubject = 'N'
					AND @ListingStatus IN ('L', 'H', 'LUV')
				)
				BEGIN
					SET @Enabled = 1
				END
            END
	END
END

SELECT @Enabled AS EquifaxEnabled</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="EquifaxEnabled">
          <DataField>EquifaxEnabled</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsDisplayPayIndicator">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 'Y' As DisplayPayIndicator
 FROM PRCompanyPayIndicator WITH (NOLOCK)
            INNER JOIN Company WITH (NOLOCK) ON prcpi_CompanyID = comp_CompanyID
WHERE prcpi_CompanyID =dbo.ufn_BRGetHQID(@CompanyID)
     AND prcpi_Current = 'Y'
     AND comp_PRIndustryType = 'L'
     AND comp_PRPublishPayIndicator = 'Y';</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
