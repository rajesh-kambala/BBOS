    <DataSet Name="dsBusinessSnapshot">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyID">
            <Value>=Parameters!CompanyID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @HQID int = dbo.ufn_BRGetHQID(@CompanyID);

DECLARE @BusinessStartDate varchar(100)
SELECT @BusinessStartDate = BusinessStartDate FROM 
(
	SELECT TOP 1 
				prbe_DisplayedEffectiveDate AS BusinessStartDate,
				prst_State AS BusinessStartState,
				MIN(prbe_EffectiveDate) prbe_EffectiveDate,
				[Priority]= CASE WHEN prbe_BusinessEventTypeId=9 THEN 1 
										     WHEN prbe_BusinessEventTypeId=42 THEN 2 
												 WHEN prbe_BusinessEventTypeId=8 THEN 3
										END
	FROM PRBusinessEvent WITH(NOLOCK)
		LEFT OUTER JOIN PRState WITH (NOLOCK) ON prbe_StateID = prst_StateID
	WHERE prbe_BusinessEventTypeId IN (9, 42, 8)
		AND ((prbe_BusinessEventTypeID &lt;&gt; 8) OR (prbe_BusinessEventTypeID=8 AND prbe_DetailedType NOT IN ('1', '2', '3')))
		AND prbe_CompanyId = @HQID
		GROUP BY prbe_CompanyID, prbe_BusinessEventTypeId, prbe_EffectiveDate, prbe_DisplayedEffectiveDate, prst_State, prbe_DetailedType
		ORDER BY prbe_CompanyID, Priority ASC
) x

SELECT @BusinessStartDate as BusinessStartDate,
       prbe_DisplayedEffectiveDate as IncorporationDate,
	   prst_State as IncorporationState,
	   prpa_LicenseNumber,
       CASE WHEN prdr_LicenseNumber IS NOT NULL THEN 'DRC Member' ELSE NULL END As prdr_LicenseNumber
  FROM Company WITH (NOLOCK)
     LEFT OUTER JOIN PRBusinessEvent becorp WITH (NOLOCK)  ON comp_CompanyID = becorp.prbe_CompanyID AND becorp.prbe_BusinessEventTypeID IN (8,9) AND prbe_DetailedType IN ('1', '2', '3')
	   LEFT OUTER JOIN PRState ON becorp.prbe_StateID = prst_StateID
	   LEFT OUTER JOIN PRPACALicense WITH (NOLOCK) ON comp_CompanyID = prpa_CompanyId AND prpa_Current = 'Y' AND prpa_Publish = 'Y'
	   LEFT OUTER JOIN PRDRCLicense  WITH (NOLOCK) ON comp_CompanyID = prdr_CompanyId AND prdr_Publish = 'Y'
     WHERE comp_CompanyID = @HQID</CommandText>
      </Query>
      <Fields>
        <Field Name="BusinessStartDate">
          <DataField>BusinessStartDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IncorporationDate">
          <DataField>IncorporationDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IncorporationState">
          <DataField>IncorporationState</DataField>
