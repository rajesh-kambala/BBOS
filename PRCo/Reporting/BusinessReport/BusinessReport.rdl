    <DataSet Name="dsCreditSnapshot">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@HQID">
            <Value>=Parameters!HQID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT prra_RatingLine, 
       prbs_BBScore, 
	   prbs_IndustryAve,
	   prbs_IndustryPercentile,
	   comp_PRFinancialStatementDate, 
	   prfs_TotalEquity as NetWorth,
	   AveIntegrity,
	   capt_us IndustryAveIntegrity, 
	   TradeReportCount,
	   prcpi_PayIndicator,
	   comp_PRConfidentialFS,
	   prra_CreditWorthId,
	   prcw_Name
  FROM Company WITH (NOLOCK)
       LEFT OUTER JOIN vPRCompanyRating ON comp_CompanyID = prra_CompanyID and prra_Current = 'Y' 
	   LEFT OUTER JOIN PRBBScore WITH (NOLOCK) ON comp_CompanyID = prbs_CompanyID and prbs_Current = 'Y' AND prbs_PRPublish = 'Y' AND comp_PRPublishBBScore='Y' 
	   LEFT OUTER JOIN PRFinancial WITH (NOLOCK) ON comp_CompanyID = prfs_CompanyID AND comp_PRFinancialStatementDate = prfs_StatementDate AND prfs_Publish = 'Y'
	   LEFT OUTER JOIN PRCompanyPayIndicator WITH (NOLOCK) ON comp_CompanyID = prcpi_CompanyID AND prcpi_Current = 'Y'
	   LEFT OUTER JOIN (SELECT prtr_SubjectID, AVG(CAST(prin_Weight as decimal(6,3))) as AveIntegrity, COUNT(prin_Weight) as TradeReportCount
	                      FROM PRTradeReport WITH (NOLOCK)
							   INNER JOIN Company C1 WITH (NOLOCK) ON C1.comp_CompanyID = prtr_ResponderId AND C1.comp_PRIgnoreTES IS NULL
						       INNER JOIN PRIntegrityRating ON prtr_IntegrityId = prin_IntegrityRatingId
						 WHERE prtr_Date &gt;= DATEADD(month, -6, GETDATE())
						   AND prtr_SubjectID = @HQID
                                                                                                         AND prtr_Duplicate IS NULL
                                                                                                         AND prtr_ExcludeFromAnalysis IS NULL
					  GROUP BY prtr_SubjectID) T1 ON comp_CompanyID = prtr_SubjectID
       LEFT OUTER JOIN custom_captions ON  capt_family = 'IndustryAverageRatings_'  + comp_PRIndustryType
		                                 AND capt_code = 'IntegrityAverage_Current'
WHERE comp_CompanyID = @HQID</CommandText>
      </Query>
      <Fields>
