  <DataSets>
    <DataSet Name="dsGetListing">
      <Fields>
        <Field Name="results">
          <DataField>results</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>select results = dbo.ufn_GetListingFromCompany(@CompanyId, 2, 1)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsCompanyInfo">
      <Fields>
        <Field Name="comp_PRBookTradestyle">
          <DataField>comp_PRBookTradestyle</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CityState">
          <DataField>CityState</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IndustryTypeDesc">
          <DataField>IndustryTypeDesc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>select comp_PRBookTradestyle, CityState = prci_City + ', ' + prst_Abbreviation,  
          IndustryTypeDesc = cast(ind.capt_US as varchar(500)) + ' Listing'  
from company   
inner join PRCity on comp_prlistingcityid = prci_CityId  
inner join PRState on prci_StateId = prst_StateId  
inner join Custom_Captions ind ON capt_family = 'comp_PRIndustryType' and capt_code = comp_PRIndustryType  
where comp_CompanyId = @CompanyId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="dsBranches">
      <Fields>
        <Field Name="comp_companyID">
          <DataField>comp_companyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT comp_companyID 
FROM company 
           INNER JOIN vPRLocation on comp_PRListingCityID = prci_CityID
WHERE comp_PRHQID=@CompanyId 
AND comp_PRType = 'B'
AND comp_prlistingstatus='L' 
ORDER BY CountryStAbbrCity</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CompanyId">
            <Value>=Parameters!CompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
