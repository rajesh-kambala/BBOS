    <DataSet Name="Waves">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT        RTRIM(WaIt_WaveItemId) AS campaignID, RTRIM(WaIt_Name) AS CampaignName
FROM            WaveItems
WHERE        (WaIt_WaveId = 22) AND (WaIt_Deleted IS NULL)
ORDER BY WaIt_WaveItemId DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="campaignID">
          <DataField>campaignID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CampaignName">
          <DataField>CampaignName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Users">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>select UserID = user_UserID, Logon = rtrim(user_logon)
from Users with (nolock)
where User_Disabled is null
order by User_Logon</CommandText>
      </Query>
      <Fields>
        <Field Name="UserID">
          <DataField>UserID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Logon">
          <DataField>Logon</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
