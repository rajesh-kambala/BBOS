  <DataSets>
    <DataSet Name="dsData">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserID">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@WaveItemID">
            <Value>=Parameters!WaveItemID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Description">
            <Value>=Parameters!Description.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @SlowPayStartDate DATETIME, @SlowPayEndDate DATETIME
SET @SlowPayStartDate = @StartDate
SET @SlowPayEndDate = DATEADD(DAY, 1, @EndDate)


--
--  Build our temp table
DECLARE @MyTable TABLE (
	CompanyID INT,
    ServiceCode VARCHAR(25)
)
--
--  Build a list of slow payer responders
INSERT INTO @MyTable
SELECT DISTINCT
       prtr_ResponderId,
       dbo.ufn_GetPrimaryService(prtr_ResponderId)
  FROM PRTradeReport
		INNER JOIN Company C1 WITH (NOLOCK) ON C1.Comp_CompanyId = prtr_SubjectId AND C1.comp_PRIgnoreTES IS NULL
		INNER JOIN PRPayRating trpy ON prtr_PayRatingId = trpy.prpy_PayRatingID
		LEFT OUTER JOIN vPRCompanyRating ON prtr_SubjectId = prra_CompanyId AND prra_Current = 'Y'
 WHERE prtr_CreatedDate BETWEEN @SlowPayStartDate AND @SlowPayEndDate
   AND prtr_PayRatingId IN (4,3,2) -- D, E, F
   AND prtr_ResponderId NOT IN (SELECT prss_ClaimantCompanyId FROM PRSSFile WHERE prss_Type='C' AND prss_Status='O') -- Has an open case
   AND prtr_SubjectId NOT IN (SELECT DISTINCT prra_CompanyId
                                FROM PRRating 
                                     LEFT OUTER JOIN PRRatingNumeralAssigned ON prra_RatingId = pran_RatingId
                               WHERE pran_RatingNumeralId IN (17, 18, 19, 86, 88, 113, 114)
                                 AND prra_Current = 'Y')
   AND prtr_SubjectId NOT IN (SELECT DISTINCT prra_CompanyId
                                FROM PRRating 
                               WHERE prra_PayRatingId = 2
                                 AND prra_Current = 'Y')
   AND prtr_ResponderId NOT IN (SELECT DISTINCT CmLi_Comm_CompanyId
				FROM vCommunication WITH (NOLOCK)
				INNER JOIN WaveItems WITH (NOLOCK) ON WaIt_WaveItemId = Comm_WaveItemId
					AND WaIt_WaveId = 22
				WHERE 
				Comm_DateTime &gt;= DATEADD(MONTH, -12, @SlowPayStartDate))


--
-- Add to the list collections prospects that
-- are not already in the list.
INSERT INTO @MyTable
SELECT DISTINCT
      prss_ClaimantCompanyId,
       dbo.ufn_GetPrimaryService(prss_ClaimantCompanyId)
  FROM PRSSFile
 WHERE prss_ClaimantCompanyId NOT IN (SELECT prss_ClaimantCompanyId FROM PRSSFile WHERE prss_Type='C' AND prss_Status='O')
   AND prss_Type='C' 
   AND prss_Status='P'
   AND prss_ClaimantCompanyId NOT IN (SELECT DISTINCT CmLi_Comm_CompanyId
		FROM vCommunication WITH (NOLOCK)
		INNER JOIN WaveItems WITH (NOLOCK) ON WaIt_WaveItemId = Comm_WaveItemId
			AND WaIt_WaveId = 22
		WHERE 
		Comm_DateTime &gt;= DATEADD(MONTH, -12, @SlowPayStartDate))
   AND prss_ClaimantCompanyId NOT IN (SELECT CompanyID FROM @MyTable);



DELETE FROM @MyTable WHERE ServiceCode IS NULL;

--
-- Select our data set to include in the spreadsheet
SELECT DISTINCT
       comp_CompanyID AS [BB ID],
       comp_name AS [Company Name],
	   ISNULL(RTRIM(Addr_Address1),'') [Mailing Address Line 1],
	   ISNULL(RTRIM(Addr_Address2),'') [Mailing Address Line 2],
	   prci_City [Mailing City],
	   ISNULL(prst_Abbreviation, prst_State) [Mailing STATE],
       prcn_Country [Mailing COUNTRY],
       Addr_PostCode [Mailing Postal Code],
       dbo.ufn_FormatPhone(Phon_CountryCode, Phon_AreaCode, Phon_Number, phon_PRExtension) AS Fax,
	   CASE 
		WHEN pHE.pers_PersonID IS NOT NULL THEN 
			pHE.pers_FirstName
        WHEN pLRL.pers_PersonID IS NOT NULL THEN 
            pLRL.pers_FirstName
		ELSE 
			'Valued Blue Book Member'
		END AS FirstName,
	   CASE 
		WHEN pHE.pers_PersonID IS NOT NULL THEN 
			pHE.pers_LastName
        WHEN pLRL.pers_PersonID IS NOT NULL THEN 
            pLRL.pers_LastName
		ELSE 
			''
		END AS LastName,
		
		'EXEC usp_CreateTask @RelatedCompanyID=' + CAST(CompanyID AS VARCHAR(10)) + 
	  CASE WHEN	ISNULL(plHE.peli_PersonID, atLRL.prattn_PersonID) IS NULL THEN '' ELSE ', @RelatedPersonID=' + CAST(ISNULL(plHE.peli_PersonID, atLRL.prattn_PersonID) AS VARCHAR(10)) END + ', ' + 
      '@Action=''FaxOut'', ' +
      '@Status=''Complete'', ' +
      '@StartDateTime=''' + CONVERT(NVARCHAR(30),@StartDate,101) + ''', ' +
      '@AssignedToUserId = ' + @UserID +', ' +
      '@PRCategory= ''SS'', ' +
      '@WaveItemID= ' + @WaveItemID + ', ' +
      '@TaskNotes= '''+ @Description + '''' AS CreateTask
  FROM @MyTable
       INNER JOIN Company ON CompanyID = comp_CompanyID
       LEFT OUTER JOIN Address_Link ON CompanyID = adli_CompanyId AND AdLi_AddressId=dbo.ufn_GetAddressIDForCAC(CompanyID)
       LEFT OUTER JOIN Address ON AdLi_AddressId = Addr_AddressID
       LEFT OUTER JOIN vPRLocation ON addr_PRCityId = prci_CityID
       LEFT OUTER JOIN PRAttentionLine atLRL ON CompanyID = atLRL.prattn_CompanyID AND atLRL.prattn_ItemCode = 'TES-M'
       LEFT OUTER JOIN Person pLRL ON atLRL.prattn_PersonID = pLRL.pers_PersonID
       LEFT OUTER JOIN Person_Link plHE ON CompanyID = plHE.peli_CompanyID AND plHE.peli_PRStatus IN (1,2) AND plHE.peli_PRRole LIKE '%,HE,%'
       LEFT OUTER JOIN Person pHE ON plHE.peli_PersonID = pHE.pers_PersonID
       LEFT OUTER JOIN vPRCompanyPhone ON CompanyID = plink_RecordID AND phon_PRIsFax = 'Y' AND phon_PRPreferredInternal = 'Y'
 WHERE comp_PRReceivePromoFaxes = 'Y'       
Order BY comp_CompanyID;</CommandText>
