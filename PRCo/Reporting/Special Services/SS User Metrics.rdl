  <DataSets>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>DECLARE @MyTable table (
    DisplaySequence int,
	Metric varchar(50),
	Logon varchar(10),
	Value int,
	LogonSequence int
)

INSERT INTO @MyTable
SELECT 1,
       'Open Claim Files', 
       RTRIM(user_logon),
       COUNT(1),
	   NULL
  FROM PRSSFile
       LEFT OUTER JOIN Users ON prss_AssignedUserId = user_userID
 WHERE prss_Type = 'C'
-- AND user_logon &lt;&gt; 'dcn'
   AND prss_Status = 'O'
GROUP BY RTRIM(user_logon)

INSERT INTO @MyTable
SELECT 2,
       'Older Than 90 Days', 
       RTRIM(user_logon),
       COUNT(1),
	   NULL
  FROM PRSSFile
       LEFT OUTER JOIN Users ON prss_AssignedUserId = user_userID
 WHERE prss_Type = 'C'
   AND prss_Status = 'O'
   AND prss_OpenedDate &lt;= DATEADD(month, -3, DATEADD(m, DATEDIFF(m, 0, GETDATE()), 0)) --First of the current month
GROUP BY RTRIM(user_logon)


INSERT INTO @MyTable
SELECT 3,
       'Assigned Last Month', 
       RTRIM(user_logon),
       COUNT(1),
	   NULL
  FROM PRSSFile
       LEFT OUTER JOIN Users ON prss_AssignedUserId = user_userID
 WHERE prss_Type = 'C'
   --AND user_logon &lt;&gt; 'dcn'
   AND prss_OpenedDate BETWEEN DATEADD(m,-1,DATEADD(mm, DATEDIFF(m,0,GETDATE()), 0)) AND DATEADD(minute, 1439, DATEADD(d, -1,DATEADD(mm, DATEDIFF(m,0,GETDATE()),0)))
GROUP BY RTRIM(user_logon)

INSERT INTO @MyTable
SELECT 4,
       'Closed Last Month', 
       RTRIM(user_logon),
       COUNT(1),
	   NULL
  FROM PRSSFile
       LEFT OUTER JOIN Users ON prss_AssignedUserId = user_userID
 WHERE prss_Type = 'C'
   --AND user_logon &lt;&gt; 'dcn'
   AND prss_Status = 'C'
   AND prss_ClosedDate BETWEEN DATEADD(m,-1,DATEADD(mm, DATEDIFF(m,0,GETDATE()), 0)) AND DATEADD(minute, 1439, DATEADD(d, -1,DATEADD(mm, DATEDIFF(m,0,GETDATE()),0)))
GROUP BY RTRIM(user_logon)

INSERT INTO @MyTable
SELECT 5,
       'Assigned YTD', 
       RTRIM(user_logon),
       COUNT(1),
	   NULL
  FROM PRSSFile
       LEFT OUTER JOIN Users ON prss_AssignedUserId = user_userID
 WHERE prss_Type = 'C'
   --AND user_logon &lt;&gt; 'dcn'
   AND prss_OpenedDate &gt;= DATEADD(yy, DATEDIFF(yy,0,getdate()), 0) 
GROUP BY RTRIM(user_logon)

INSERT INTO @MyTable
SELECT 6,
       'Closed YTD', 
       RTRIM(user_logon),
       COUNT(1),
	   NULL
  FROM PRSSFile
       LEFT OUTER JOIN Users ON prss_AssignedUserId = user_userID
 WHERE prss_Type = 'C'
   --AND user_logon &lt;&gt; 'dcn'
   AND prss_ClosedDate &gt;= DATEADD(yy, DATEDIFF(yy,0,getdate()), 0) 
GROUP BY RTRIM(user_logon)


UPDATE @MyTable
  SET LogonSequence = CASE Logon 
						WHEN 'ccs' THEN 1 
						WHEN 'gaf' THEN 2 
						WHEN 'aja' THEN 3 
						WHEN 'dcn' THEN 4 
					END


SELECT * 
 FROM @MyTable
 ORDER BY DisplaySequence, LogonSequence</CommandText>
