    <DataSet Name="dsBatches">
      <Query>
        <DataSourceName>LumberLeads</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Source">
            <Value>=Parameters!Source.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Rating">
            <Value>=Parameters!Rating.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Description">
            <Value>=Parameters!Description.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ARDetailCountThreshold">
            <Value>=Parameters!ARDetailCountThreshold.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LastARDateThreshold">
            <Value>=Parameters!LastARDateThreshold.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ARUniqueSubmitterThreshold">
            <Value>=Parameters!ARUniqueSubmitterThreshold.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ExcludeExportedLeads">
            <Value>=Parameters!ExcludeExportedLeads.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ExcludeHandledInternallyLeads">
            <Value>=Parameters!ExcludeHandledInternallyLeads.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ExcludeBatchesWOPhone">
            <Value>=Parameters!ExcludeBatchesWOPhone.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ExcludeClosedLeads">
            <Value>=Parameters!ExcludeClosedLeads.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MustHaveAnOpenLead">
            <Value>=Parameters!MustHaveAnOpenLead.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyName">
            <Value>=Parameters!CompanyName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PhoneNumber">
            <Value>=Parameters!PhoneNumber.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BatchCountThreshold">
            <Value>=Parameters!BatchCountThreshold.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BatchCountMaxThreshold">
            <Value>=Parameters!BatchCountMaxThreshold.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @CLTable table (
	BatchID int primary key
)

DECLARE @ARTable table (
	BatchID int primary key
)

DECLARE @RBTable table (
	BatchID int primary key
)

DECLARE @MyTable table (
	BatchID int primary key
)

IF ('Connection List' IN (@Source)) BEGIN
    INSERT INTO @CLTable
	SELECT DISTINCT Match.BatchID
	  FROM Lead
		   INNER JOIN Match ON Lead.LeadID = Match.LeadID
     WHERE Source = 'Connection List';
END


IF ('Redbook' IN (@Source)) BEGIN
	Print 'RB'
    INSERT INTO @RBTable
	SELECT DISTINCT Match.BatchID
	  FROM Lead
	       INNER JOIN Match ON Lead.LeadID = Match.LeadID
            LEFT OUTER JOIN RedbookDescription ON Lead.LeadID = RedbookDescription.LeadID
	 WHERE Source = 'Redbook'
           AND Rating IN (@Rating)
           AND Description LIKE ISNULL(@Description, Description);
END


IF ('Unmatched AR' IN (@Source)) BEGIN
	Print 'AR'
    INSERT INTO @ARTable
	SELECT DISTINCT Match.BatchID
	  FROM Lead
	       INNER JOIN Match ON Lead.LeadID = Match.LeadID
	 WHERE Source = 'Unmatched AR'
             AND ARDetailCount &gt;= @ARDetailCountThreshold
            AND LastARDate &gt;= @LastARDateThreshold;


	IF (@ARUniqueSubmitterThreshold &gt; 1) BEGIN

	DELETE FROM @ARTable
         WHERE BatchID NOT IN (
			SELECT Match.BatchID
			  FROM Lead
				   INNER JOIN Match ON Lead.LeadID = Match.LeadID
  			 WHERE Source = 'Unmatched AR'
			GROUP BY Match.BatchID
			HAVING COUNT(DISTINCT AssociatedCompanyID) &gt;= @ARUniqueSubmitterThreshold);
	END
END


INSERT INTO @MyTable
SELECT DISTINCT BatchID FROM
(
	SELECT BatchID FROM @CLTable
    UNION
	SELECT BatchID FROM @ARTable
	UNION
	SELECT BatchID FROM @RBTable
) T1;

IF (@ExcludeExportedLeads = 'True') BEGIN

     DELETE FROM @MyTable
     WHERE BatchID IN (
         SELECT DISTINCT BatchID
          FROM Lead
	    INNER JOIN Match ON Lead.LeadID = Match.LeadID
          WHERE Lead.ExportedForValidation = 'Y');

END

IF (@ExcludeHandledInternallyLeads = 'True') BEGIN

     DELETE FROM @MyTable
     WHERE BatchID IN (
         SELECT DISTINCT BatchID
          FROM Lead
	    INNER JOIN Match ON Lead.LeadID = Match.LeadID
          WHERE Lead.HandingInternally = 'Y');

END

IF (@ExcludeBatchesWOPhone = 'True') BEGIN

    DELETE FROM @MyTable
    WHERE BatchID NOT  IN (
	SELECT DISTINCT BatchID 
                  FROM Lead 
                              INNER JOIN Match ON Lead.LeadID = Match.LeadID 
                WHERE PhoneNumber IS NOT NULL 
                     AND PhoneNumber NOT LIKE '%E+%'
                     AND BatchID IS NOT NULL);

END

IF (@ExcludeClosedLeads = 'True') BEGIN

     DELETE FROM @MyTable
     WHERE BatchID IN (
         SELECT DISTINCT BatchID
          FROM Lead
	    INNER JOIN Match ON Lead.LeadID = Match.LeadID
          WHERE Lead.Status NOT IN ('Open', 'Reopened'));
END 


IF (@MustHaveAnOpenLead = 'True') BEGIN

     DELETE FROM @MyTable
     WHERE BatchID IN (	SELECT BatchID
	  FROM (
         SELECT BatchID, COUNT(1) LeadCount, COUNT(CASE WHEN Lead.Status IN ('Open', 'Reopened') THEN 'x' ELSE NULL END) As OpenCount
          FROM Lead
	           INNER JOIN Match ON Lead.LeadID = Match.LeadID
		GROUP BY BatchID) T1
	WHERE OpenCount = 0);

END



SELECT DISTINCT Match.BatchID, BatchCount, ISNULL(OpenCount, 0) OpenCount, CompanyName, dbo.ufn_GetBatchPhoneNumber(Match.BatchID) As PhoneNumber, dbo.ufn_GetBatchFaxNumber(Match.BatchID) As FaxNumber, dbo.ufn_GetCustomerNumber(Match.BatchID) As CustomerNumber,
CASE WHEN CompanyName = @CompanyName THEN 'Y' ELSE 'N' END as ExactMatch
  FROM Lead
       INNER JOIN Match ON Lead.LeadID = Match.LeadID
       INNER JOIN @MyTable mt ON Match.BatchID = mt.BatchID
       INNER JOIN (SELECT BatchID, COUNT(1) As BatchCount FROM Match GROUP BY BatchID) T1 ON Match.BatchID = T1.BatchID
      INNER JOIN (SELECT DISTINCT BatchID 
                             FROM Lead  
		        INNER JOIN Match ON Lead.LeadID = Match.LeadID
WHERE CompanyNameMatch LIKE '%' +  CRM.dbo.ufn_GetLowerAlpha(LumberLeads.dbo.ufn_PrepareCompanyName(@CompanyName))  + '%' 
  AND PhoneMatch LIKE '%' + CRM.dbo.ufn_GetLowerAlpha(@PhoneNumber) + '%'
		) T2 ON Match.BatchID = T2.BatchID
     LEFT OUTER JOIN (SELECT BatchID, COUNT(DISTINCT Match.LeadID) As OpenCount FROM Match INNER JOIN Lead ON Match.LeadID = Lead.LeadID WHERE Status IN ('Open', 'Reopened') GROUP BY BatchID) T3 ON Match.BatchID = T3.BatchID
 WHERE MatchedLeadID IS NULL
   AND BatchCount &gt;= @BatchCountThreshold
   AND BatchCount &lt;= @BatchCountMaxThreshold
ORDER BY CASE WHEN CompanyName = @CompanyName THEN 'Y' ELSE 'N' END DESC,  BatchCount DESC; </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
