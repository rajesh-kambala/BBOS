  <DataSets>
    <DataSet Name="BluePrintsEdition">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>SELECT RTRIM(capt_code) AS capt_code, capt_us FROM custom_captions WHERE capt_family = 'pradc_BlueprintsEdition' ORDER BY capt_order</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="capt_code">
          <DataField>capt_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="capt_us">
          <DataField>capt_us</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Results">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BlueprintsEdition">
            <Value>=Parameters!BlueprintsEdition.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @Parm1 varchar(40)
SET @Parm1 = '%' + @BlueprintsEdition + '%'

SELECT 
	[BBID] = comp_CompanyID
	, [Company Name] = comp_Name
	, [Listing City] = prci_City
	, [Listing State] = prst_State
	, [Listing Country] = prcn_Country
	, [Sales Territory] = left(prci_SalesTerritory, 2)
	, [Ad Campaign Name] = pradc_Name
	, [Ad Size] = dbo.ufn_GetCustomCaptionValue('pradc_AdSize', pradc_AdSize, 'en-us')
	, [Ad Color] = dbo.ufn_GetCustomCaptionValue('pradc_AdColor', pradc_AdColor, 'en-us')
	, [Orientation] = dbo.ufn_GetCustomCaptionValue('pradc_Orientation', pradc_Orientation, 'en-us')
	, [Sold By] = rtrim(User_Logon)
	, [Approved By] = dbo.ufn_FormatPerson(pers_FirstName, pers_LastName, pers_MiddleName, NULL, pers_Suffix)
	, [Creative Status] = dbo.ufn_GetCustomCaptionValue('pradc_CreativeStatus', pradc_CreativeStatus, 'en-us')
	, [Ad Placement] = dbo.ufn_GetCustomCaptionValue('pradc_Placement', pradc_Placement, 'en-us')
	, [Section] = dbo.ufn_GetCustomCaptionValue('pradc_Section',  pradc_Section, 'en-us')
	, [Planned Section] = dbo.ufn_GetCustomCaptionValue('pradc_PlannedSection', pradc_PlannedSection, 'en-us')
	, [Placement Instructions] = pradc_PlacementInstructions
	, [Notes] = pradc_Notes
	, [Section Detail] = pradc_SectionDetails
	, [On Terms?] = pradch_Terms
	, [Terms Details] = pradch_TermsDescription
	, [Last Updated] = pradc_UpdatedDate
	, [Is Premium?] = pradc_Premium
	, [Is Renewal?] = pradch_Renewal
	, [Level 1 Class] = dbo.ufn_GetLevel1Classifications(comp_CompanyID, 2)
	, [Source Request] = pradc_ContentSourceRequest
	, [Source Request Date] = pradc_ContentSourceRequestDate,

	pradc_ContentSourceContactName,
	pradc_ContentSourceContactInfo
FROM PRAdCampaignHeader
INNER JOIN PRAdCampaign WITH (NOLOCK) ON pradc_AdCampaignHeaderID = pradch_AdCampaignHeaderID
INNER JOIN Company WITH (NOLOCK) ON pradc_CompanyID = comp_CompanyID
INNER JOIN vPRLocation with (nolock) on prci_CityID = comp_PRListingCityID
LEFT OUTER JOIN Users WITH (NOLOCK) ON user_userid = pradch_SoldBy
LEFT OUTER JOIN Person WITH (NOLOCK) ON pers_PersonID = pradch_ApprovedByPersonID

WHERE pradc_BluePrintsEdition LIKE @Parm1
ORDER BY comp_Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
