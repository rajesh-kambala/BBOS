  <DataSets>
    <DataSet Name="KYCEditions">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <CommandText>select Capt_Code, Capt_US from Custom_Captions where Capt_Family='pradc_kycedition' order by capt_code desc</CommandText>
      </Query>
      <Fields>
        <Field Name="Capt_Code">
          <DataField>Capt_Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Capt_US">
          <DataField>Capt_US</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsResults">
      <Query>
        <DataSourceName>BBS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@KYCEdition">
            <Value>=Parameters!KYCEdition.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT comp_CompanyID,
       pradc_AdCampaignID,
       comp_Name = comp_PRCorrTradestyle,
       pradc_Name,
       ISNULL(prkycc_PostName,'') [Publication Article], 
       dbo.ufn_GetCustomCaptionValue('pradc_Placement', pradc_Placement, 'en-us') As pradc_Placement,
       User_Logon As SoldBy,
       dbo.ufn_FormatPerson(pers_FirstName, pers_LastName, pers_MiddleName, NULL, pers_Suffix) As ApprovedBy,

       dbo.ufn_GetCustomCaptionValue('pradc_CreativeStatus', pradc_CreativeStatusPrint, 'en-us') As pradc_CreativeStatus,

       ISNULL(pradc_Notes,'') as pradc_Notes,
       left(prci_SalesTerritory,2) as [SalesTerritory],
      Premium = pradc_premium,
      Renewal = pradch_renewal
			,[Alternate Trade Name] = pradch_AltTradeName
			, [Member] = CASE WHEN prse_ServiceCode IS NULL THEN 'N' ELSE 'Y' END
			, [Trading/Transportation Member ] = ISNULL(comp_PRTMFMAward, 'N')
			, pradc_KYCEdition
			, pradc_AdCampaignType
			, dbo.ufn_GetCustomCaptionValue('pradc_AdSize', pradc_AdSize, 'en-us') As pradc_AdSize
			, ISNULL(pradc_placementinstructions, '') as pradc_placementinstructions
FROM PRAdCampaignHeader
		INNER JOIN PRAdCampaign WITH (NOLOCK) ON pradch_AdCampaignHeaderID = pradc_AdCampaignHeaderID
    INNER JOIN Company WITH (NOLOCK) ON pradc_CompanyID = comp_CompanyID
    INNER JOIN vPRLocation ON comp_PRListingCityID = prci_CityID
    LEFT OUTER JOIN Person WITH (NOLOCK) ON pers_PersonID = pradch_ApprovedByPersonID
	  LEFT OUTER JOIN PRService ON prse_CompanyID = comp_CompanyID AND Category2 ='Primary'
	  LEFT OUTER JOIN custom_captions placement WITH (NOLOCK)  ON pradc_Placement = placement.capt_code AND placement.capt_family = 'pradc_Placement'
	  LEFT OUTER JOIN custom_captions creative WITH (NOLOCK)  ON pradc_CreativeStatus = creative.capt_code AND creative.capt_family = 'pradc_CreativeStatus'
	  LEFT OUTER JOIN users WITH (NOLOCK) ON pradch_SoldBy = user_userID
		LEFT OUTER JOIN PRKYCCommodity ON pradc_KYCCommodityID = prkycc_KYCCommodityID
WHERE pradch_TypeCode = 'KYC'
	 AND pradc_KYCEdition = @KYCEdition
	 AND pradc_CreativeStatusPrint IS NOT NULL
ORDER BY comp_PRCorrTradestyle</CommandText>
      </Query>
